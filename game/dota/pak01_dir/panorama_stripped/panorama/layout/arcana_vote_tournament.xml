<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/compendiums/compendium_styles.css" />
		<include src="file://{resources}/styles/arcana_vote_tournament.css" />
	</styles>
				  
  <ArcanaVoteTournament>
	<Panel id="Connections" hittest="false">
	                        
	</Panel> 
	<Panel id="Heroes" hittest="false">
	                        
	</Panel>
	  <Panel id="BracketOverview">
		  <Panel id="BracketOverviewImage">
			  <Panel class="BracketQuadHighlight"></Panel>
			  <Panel class="BracketHighlightButton Quad0" onactivate="DOTAArcanaSetZoom( 1, 0 )"></Panel>
			  <Panel class="BracketHighlightButton Quad1" onactivate="DOTAArcanaSetZoom( 1, 1 )"></Panel>
			  <Panel class="BracketHighlightButton Quad2" onactivate="DOTAArcanaSetZoom( 1, 2 )"></Panel>
			  <Panel class="BracketHighlightButton Quad3" onactivate="DOTAArcanaSetZoom( 1, 3 )"></Panel>
		  </Panel>
		  <Panel class="FullBracketButton" onactivate="DOTAArcanaSetZoom( 0, 0 )">
			  <Label text="#DOTA_ArcanaVote_ViewFullBracket" />
		  </Panel>
	  </Panel>
      <Label class="BracketGroup GroupA" text="#DOTAArcanaVote_Group_A" />
      <Label class="BracketGroup GroupB" text="#DOTAArcanaVote_Group_B" />
      <Label class="BracketGroup GroupC" text="#DOTAArcanaVote_Group_C" />
      <Label class="BracketGroup GroupD" text="#DOTAArcanaVote_Group_D" />
  </ArcanaVoteTournament>
</root>
