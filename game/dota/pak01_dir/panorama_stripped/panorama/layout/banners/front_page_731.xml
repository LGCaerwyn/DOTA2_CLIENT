<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/frontpage_shared.css" />
		<include src="file://{resources}/styles/banners/front_page_731.css" />
	</styles>

	<CustomLayoutPanel class="GameplayPatchCell" onactivate="DOTAShowPatchVersion( '7.31' )">
		<Image id="CellImage" class="FrontpageCellImage" src="file://{images}/front_page/731_update.psd" scaling="stretch-to-cover-preserve-aspect" />
		<Label class="UpdateTitle" text="7.31" />
		<DOTAScenePanel id="UpdateTitleEffect" map="scenes/rank_tier_ambient" camera="shot_camera" particleonly="true" hittest="false" renderdeferred="true" deferredalpha="true" />
		<Label class="UpdateSubTitle" text="#DOTA_PatchNotes_Header" />
	</CustomLayoutPanel>
</root>
