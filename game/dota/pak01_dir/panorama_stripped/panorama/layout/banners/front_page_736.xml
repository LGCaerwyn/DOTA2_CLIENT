<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/frontpage_shared.css" />
		<include src="file://{resources}/styles/battle_pass/crownfall/bp_crownfall_shared.css" />
		<include src="file://{resources}/styles/banners/front_page_736.css" />
	</styles>

	<CustomLayoutPanel class="FrontPageCell736" onactivate="">
		<Image class="CellArt" src="file://{images}/front_page/gameplay_736.png" scaling="stretch-to-fit-preserve-aspect" />
		<Panel class="CellBorder"/>
		<Label class="CellLabel LabelSmall" text="#DOTA_PatchNotes_Header" />
		<Panel class="CellContent">
			<Label class="PatchTitle TitleExtraLarge" text="#DOTA_FrontPageCell_736_Title" />
			<Label class="Description" text="#DOTA_FrontPageCell_736_Description" />
			<DOTACrownfallButton id="" class="" text="#DOTA_FrontPageCell_736_Patchnotes" onactivate="DOTAShowPatchVersion( '7.36' )" />
			<Label class="VisitWebsiteLabel LabelMedium" text="#DOTA_Crownfall_FrontPage_VisitWebsite" onactivate="ExternalBrowserGoToDOTAURL( patches/7.36 )"/>
		</Panel>
		                                     
			                                                                                                                                                            
		            
	</CustomLayoutPanel>
</root>
