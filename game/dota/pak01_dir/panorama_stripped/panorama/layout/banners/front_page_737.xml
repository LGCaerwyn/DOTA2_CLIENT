<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/frontpage_shared.css" />
		<include src="file://{resources}/styles/battle_pass/crownfall/bp_crownfall_shared.css" />
		<include src="file://{resources}/styles/banners/front_page_737.css" />
	</styles>

	<CustomLayoutPanel class="Patch_737" onactivate="DOTAShowPatchVersion( '7.37' )">
		<Image class="CellArt" src="file://{images}/front_page/front_page_737.png" scaling="stretch-to-fit-preserve-aspect" />
		                                     
		<Panel class="CellBorder"/>
		<Label class="CellTitle LabelMedium" text="#DOTA_FrontPageCell_737_GameplayUpdate" />
		<Panel class="CellContent">
			<Label class="PatchTitle TitleSmall" text="#DOTA_FrontPageCell_737_Title" />
			<Label class="Description" text="#DOTA_FrontPageCell_737_Description" />
			<DOTACrownfallButton class="Secondary" text="#DOTA_Frontpage_GameplayPatch_Button" onactivate="DOTAShowPatchVersion( '7.37' )" />
		</Panel>
	</CustomLayoutPanel>
</root>
