<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/frontpage_shared.css" />
		<include src="file://{resources}/styles/banners/front_page_738.css" />
	</styles>

	<CustomLayoutPanel class="FrontPageCell738" onactivate="">
		<Image class="CellArt" src="file://{images}/front_page/gameplay_738.png" scaling="stretch-to-fit-preserve-aspect" />
		<Panel class="CellBorder"/>
		<Panel class="CellContent">
			<Label class="CellLabel LabelMedium" text="#DOTA_FrontPageCell_738_Header" html="true"/>
			<Label class="PatchTitle TitleMedium" text="#DOTA_FrontPageCell_738_Title" />
			<Label class="Description" text="#DOTA_FrontPageCell_738_Description" />
		</Panel>
		<Panel class="ButtonsContainer">
			<Button id="PatchNotesButton" class="ButtonPrimary Large Teal" onactivate="DOTAShowPatchVersion( '7.38' )">
				<Label text="#DOTA_FrontPageCell_738_Patchnotes" />
			</Button>
			<Label class="VisitWebsiteLabel LabelMedium" text="#DOTA_Crownfall_FrontPage_VisitWebsite" onactivate="ExternalBrowserGoToDOTAURL( 'wanderingwaters' )"/>
			                                                                                                                                                                     
		</Panel>
	</CustomLayoutPanel>
</root>
