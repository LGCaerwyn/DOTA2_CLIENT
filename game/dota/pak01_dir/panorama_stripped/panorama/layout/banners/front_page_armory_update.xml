<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/frontpage_shared.css" />
		<include src="file://{resources}/styles/banners/front_page_armory_update.css" />
	</styles>

	<CustomLayoutPanel class="ArmoryUpdateCell" onactivate="ExternalBrowserGoToDOTAURL( 'summer2023' )">
		<Image class="FrontpageCellImage" src="file://{images}/front_page/augupdate23.psd" scaling="stretch-to-cover-preserve-aspect" />
		<DOTAScenePanel class="UpdateTitleEffect" map="scenes/rank_tier_ambient" camera="shot_camera" particleonly="true" hittest="false" renderdeferred="true" deferredalpha="true" />
		<Panel class="TextArea">
			<Label class="UpdateTitle" text="#DOTA_SummerUpdate2023_Title" />
			<Label class="UpdateNote" text="#DOTA_SummerUpdate2023_Armory" />
			<Label class="UpdateNote" text="#DOTA_SummerUpdate2023_Behavior" />
			<Label class="UpdateNote" text="#DOTA_SummerUpdate2023_Rendering" />
		</Panel>
	</CustomLayoutPanel>
</root>
