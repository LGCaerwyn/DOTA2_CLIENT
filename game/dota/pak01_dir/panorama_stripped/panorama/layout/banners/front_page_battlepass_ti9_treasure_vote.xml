<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/frontpage_shared.css" />
		<include src="file://{resources}/styles/battle_pass/international2019/bp_international2019_shared.css" />
		<include src="file://{resources}/styles/banners/front_page_battlepass_ti9_treasure_vote.css" />
	</styles>

	<CustomLayoutPanel class="background" onactivate="DOTAShowBPInternational2019CollectorsCacheVotePage()">
		<DOTACountdown id="Countdown" end-time="1559577600" server-time="true">
			<Label class="time-left" text="{t:d:t:T:countdown_time}" />
			<Label class="remaining" text="#DOTA_Collectors_Cache_Vote_Remaining" />
			<Label class="finished" text="#DOTA_Collectors_Cache_Vote_Finished" />
		</DOTACountdown>

		<Label class="FrontpageCellHeader" text="#DOTA_BattlePass_WindowTitle" />

		<Panel class="content">
			<Label class="treasure-vote-title" text="#DOTA_Collectors_Cache_Vote_Title" />
			<Label class="treasure-vote-desc" text="#DOTA_Collectors_Cache_Vote_Desc" />
		</Panel>	
	</CustomLayoutPanel>

</root>
