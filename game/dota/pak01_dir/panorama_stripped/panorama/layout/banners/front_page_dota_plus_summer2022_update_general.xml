<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/banners/front_page_dota_plus_summer2022_update_general.css" />
	</styles>

	<CustomLayoutPanel class="DotaPlusGeneralUpdate" onactivate="BrowserGoToDOTAURL( battlereport )">
		<DOTAScenePanel id="PlusLogo" camera="shot_camera" map="scenes/plus_logo" renderdeferred="true" deferredalpha="true" antialias="true" hittest="false" particleonly="false" live-mode="high_end_only"/>
		<Panel class="GeneralUpdateTextContainer">
			<Label class="GeneralUpdateTitle" text="#DOTA_PlusDBPage_Title" />
			<Panel class="GeneralUpdateSoftEdgeDash" />
			<Label class="GeneralUpdateDescription" text="#FrontPage_DotaPlusUpdates_Summer2022_GeneralUpdate_Description" />
		</Panel>
	</CustomLayoutPanel>
</root>
