<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/frontpage_shared.css" />
		<include src="file://{resources}/styles/banners/front_page_frostivus_2023_shared.css" />
		<include src="file://{resources}/styles/banners/front_page_frostivus_2023_dotaplus.css" />
	</styles>

	<CustomLayoutPanel class="Frostivus2023Cell" onactivate="DOTAShowPlusHomePage()">
		<Image class="CellBackground" src="file://{images}/events/frostivus/2023/front_page_cell_bg.png" scaling="stretch-to-cover-preserve-aspect" />
		<Panel class="CellImage"/>
		<Panel class="SnowBorder"/>
		<Panel class="Contents">
			<Label class="SubHeader LabelSmall" text="#DOTA_Frostivus_2023_Frontpage_CellTitle" />
			<Label class="Header" text="#DOTA_Frostivus_2023_Frontpage_DotaPlus_Title" />
			<Label class="Desc" text="#DOTA_Frostivus_2023_Frontpage_DotaPlus_Description" />
		</Panel>
	</CustomLayoutPanel>
</root>
