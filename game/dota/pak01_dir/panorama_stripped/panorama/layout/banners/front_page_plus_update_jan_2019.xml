<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/frontpage_shared.css" />
		<include src="file://{resources}/styles/banners/front_page_plus_update_jan_2019.css" />
	</styles>

	<CustomLayoutPanel class="DotaPlusFrontpageCell" onactivate="DOTAPlusShowDefaultPage()">
		<Panel class="FrontpageCellImage" scaling="stretch-to-cover-preserve-aspect" />
		<Label class="FrontpageCellHeader" text="#FrontPage_DotaPlusUpdates_Header" />
		<Panel class="FrontpageCellTint" />
		<Panel class="FrontpageCellFooter">
			<Label class="FrontpageCellText" text="#FrontPage_DotaPlus_Jan2019_Text" html="true" />
			<Label class="FrontpageCellTitle" text="#FrontPage_DotaPlus_Jan2019_Title" html="true" />
		</Panel>

		<DOTAScenePanel id="PlusLogoScene" camera="shot_camera" map="scenes/plus_logo" renderdeferred="true" deferredalpha="true" antialias="true" hittest="false" particleonly="false"/>
		
	</CustomLayoutPanel>
</root>
