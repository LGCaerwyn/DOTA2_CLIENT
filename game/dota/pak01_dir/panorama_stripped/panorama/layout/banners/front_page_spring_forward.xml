<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/frontpage_shared.css" />
		<include src="file://{resources}/styles/banners/front_page_spring_forward.css" />
	</styles>

	<CustomLayoutPanel class="FrontPageSpringForward" onactivate="ExternalBrowserGoToDOTAURL( 'springforward2025' )">
		<Image class="CellArt" src="file://{images}/front_page/front_page_spring_forward.jpg" scaling="stretch-to-fit-preserve-aspect" />
		<Panel class="TopShadow"/>
		<Panel class="CellBorder"/>
		<Panel class="CellContent">
			<Label class="CellTitle TitleMedium" text="#DOTA_FrontPageCell_SpringForward_Header" />
			<Label class="Description" text="#DOTA_FrontPageCell_SpringForward_Description" />
		</Panel>
		<Label class="CellLabel LabelMedium" text="#DOTA_Visit_Website" />
	</CustomLayoutPanel>
</root>
