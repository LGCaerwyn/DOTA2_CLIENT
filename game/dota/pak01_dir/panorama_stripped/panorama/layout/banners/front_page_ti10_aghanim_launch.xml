<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/frontpage_shared.css" />
		<include src="file://{resources}/styles/banners/front_page_ti10_aghanim_launch.css" />
	</styles>

	<CustomLayoutPanel class="BattlePassCell FrontpageCell" onactivate="DOTAShowBPInternational2020EventGamePage()">


		                                                   
		                                                                                                                                                                      
		                                    
						 
		                                    
		<Panel class="LeftContent TopBottomFlow">
			<TextButton class="BattlePassButton BuyBattlePassButton" text="#DOTA_Artifact_Promo_WatchTrailer" onactivate="DOTAShowSplashAdMovie( 'aghanims_labyrinth_movie' )" />
		</Panel>
		
		<Panel class="RightContent TopBottomFlow">
			                              
			<Panel id="PopupAghanimsLogo" >
				<DOTAScenePanel id="PopupAghanimsLogoFX" camera="shot_cameraA" map="scenes/battlepass_ti10/aghslogofx" particleonly="true"/>
			</Panel>

			<Label class="aghs-subtitle" text="#TI10_EventGame_Frontpage_Subtitle" />
			<Label class="aghs-feature" text="#TI10_EventGame_Frontpage_Feature1" />
			<Label class="aghs-feature" text="#TI10_EventGame_Frontpage_Feature2" />
			<Label class="aghs-feature" text="#TI10_EventGame_Frontpage_Feature3" />
			<Label class="aghs-feature" text="#TI10_EventGame_Frontpage_Feature4" />
			<TextButton id="ViewMarsLoadout" class="BattlePassButton BuyBattlePassButton" onactivate="BrowserGoToURL( https://www.dota2.com/aghanim )" text="#TI10_EventGame_Frontpage_Button"/>

		</Panel>

		








	</CustomLayoutPanel>
</root>
