<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/battle_pass/current_battle_pass.css" />
		<include src="file://{resources}/styles/battle_pass/coach_rating.css" />
	</styles>

	<snippets>
		<snippet name="RatingProgress">
			<Panel class="RatingProgress">
				<CircularProgressBar id="RatingProgressBar" />
				<Label class="RatingBadgeValue" text="{d:coach_rating}" />
			</Panel>
		</snippet>
		<snippet name="RatingBadge">
			<Panel class="RatingBadge">
				<Label class="RatingBadgeValue" text="{d:coach_rating}" />
			</Panel>
		</snippet>
		<snippet name="RatingIcon">
			<Panel class="RatingIcon">

			</Panel>
		</snippet>
	</snippets>

	<Panel>
		                                                        
	</Panel>
</root>
