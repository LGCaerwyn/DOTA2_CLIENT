<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/dashboard_page_shared.css" />
		<include src="file://{resources}/styles/battle_pass/crownfall/bp_crownfall_shared.css" />
		<include src="file://{resources}/styles/battle_pass/crownfall/bp_page_crownfall_archive.css" />
	</styles>
	<snippets>
	</snippets>
	<DOTABPCrownfallArchivePage class="DashboardPage" oncancel="RemoveStyle( FishingGamesPopupContainer, FishingPopupOpen )">
		<Panel class="BackgroundImageContainer">
			<Panel class="BackgroundImage Act4"/>
		</Panel>
		
		<Panel class="HeroesSceneModels" hittest="false">
			<DOTAUIEconSetPreview id="PremiumReward_Skywrath" activity-modifier="takeover" itemstyle="0" class="PremiumReward CrownfallDBArcanaLeft" itemdef="22277" cameraOrigin="0 0 0" rotation="-40" drawbackground="false" require-composition-layer="true" suppress-intro-effects="true" />
			<DOTAUIEconSetPreview id="PremiumReward_Vengeful" activity-modifier="takeover" itemstyle="0" class="PremiumReward CrownfallDBArcanaRight" itemdef="22722" cameraOrigin="0 0 0" rotation="-10" drawbackground="false" require-composition-layer="true" suppress-intro-effects="true" />

			                                                                                                                                                                                                                                                                                                
			                                                                                                                                                                                                                                                                                         

		</Panel>

		<DOTAParticleScenePanel class="SmokeFX" hittest="false" particleName="particles/ui/battle_pass/ui_bp_2022_diretide_home_background.vpcf" require-composition-layer="true" renderdeferred="true" particleonly="false" startActive="true" cameraOrigin="200 0 0" lookAt="0 0 0" fov="60" squarePixels="true" />
		<Panel class="BottomGradient" />

		<Panel class="MainContents" hittest="false">
			                                               
			<Panel class="CrownfallLogoContainer" hittest="false" >
				<MoviePanel id="CrownfallLogo" repeat="true" autoplay="onload" />

			<Panel class="DashDotDivider DashDotDividerArchive">
						<Panel class="Dash Left"/>
						<Label class="GoldTitle" text="Archive" />
						<Panel class="Dash Right"/>
			</Panel>

			                                                                                      

		</Panel>

			
			                                                            
				                           
					                              
						                          
						                                                                 
						                           
					        
					                                  
						                         
							                                                                      
							                                                                           
								                                                                                                                            
								                                                                                                                   
								                                                                              
								                                                  
							        
							                                                                            
								                                                                                                                            
								                                                                                                                   
								                                                                             
								                                                  
							        
						        
						                         
							                                                                      
							                                                                     
								                                                                                                                            
								                                                                                                                   
								                                                                                       
								                                                  
							        
							                                                                      
								                                                                                                                            
								                                                                                                                   
								                                                                              
								                                                  
							        
							                                                                     
								                                                                                                                            
								                                                                                                                   
								                                                                                   
								                                                  
							        
						        
						                         
							                                                                      
							                                                                     
								                                                                                                                            
								                                                                                                                   
								                                                                           
								                                                  
							        
							                                                                     
								                                                                                                                            
								                                                                                                                   
								                                                                                
								                                                  
							        
						        
						                         
							                                                                      
							                                                                     
								                                                                                                                            
								                                                                                                                   
								                                                                           
								                                                  
							        
							                                                                               
								                                                                                                                            
								                                                                                                                   
								                                                                                
								                                                  
							        
							                                                                       
								                                                                                                                            
								                                                                                                                   
								                                                                                 
								                                                  
							        
						        
					        
					                                                   
					                                            
					                                                   
				        
				                              
					                              
						                          
						                                                                    
						                           
					        
					                            
						                                                                                                       
							                                                                        
								                             
								                                                  
								                                                 
								                                          
							        
							                                                                               
						        
						                                                                           
							                                                                                     
								                             
								                                                  
								                                                 
								                                          
							        
							                                                                                
						        
						                                                                               
							                                                                         
								                             
								                                                  
								                                                 
								                                          
							        
							                                                                                    
						        
					        
					                            
						                                                                              
							                                                                        
								                             
								                                                  
								                                                 
								                                          
							        
							                                                                                   
						        
						                                                                           
							                                                                      
								                             
								                                                  
								                                                 
								                                          
							        
							                                                                                 
						        
						                                                                           
							                                                                          
								                             
								                                                  
								                                                 
								                                          
							        
							                                                                                
						        
					        
					                            
						                                                                                
							                                                                          
								                             
								                                                  
								                                                 
								                                          
							        
							                                                                                     
						        
						                                                                                
							                                                                            
								                             
								                                                  
								                                                 
								                                          
							        
							                                                                                     
						        
						                                                                               
							                                                                          
								                             
								                                                  
								                                                 
								                                          
							        
							                                                                                    
						        
					        
					                                                   
					                                            
					                                                   
				        
			        
		
			                       
				                                      
					                          
					                                                                                                                                            
					                                                                                                                                                           
					                           
				        
			            
		</Panel>

		<Panel id="FishingGamesPopupContainer">
			<Panel class="FishingGamesPopupBlocker" hittest="true" onactivate="ToggleStyle( FishingGamesPopupContainer, FishingPopupOpen )"/>
			<Panel class="FishingGamesPopupContent">
				<Button id="CloseButton" class="ButtonPrimary DarkGrey" onactivate="ToggleStyle( FishingGamesPopupContainer, FishingPopupOpen )">
					<Image src="file://{images}/control_icons/24px/x_close.svg" />
				</Button>
				<Panel class="FishingGamesPopupContentBorder" hittest="false" />
				<Label class="GoldTitle FishingMiniGamesTitle" text="#DOTA_Crownfall_Archive_Fishing_Minigames"/>
				<Panel class="MinigamesRow">
					<Panel class="FishingButton" onactivate="DOTACrownfallMinigameFishing()">
						<Image class="FishingMinigameThumbnailImage" src="file://{images}/events/crownfall/archive/fishing.png" scaling="stretch-to-fit-preserve-aspect"></Image>					
						<Panel class="Node MainQuestNode EncounterNode fishing" hittest="false">
							<Panel id="NodeBackground" />
							<Panel id="NodeBackgroundInner" hittest="false" />
							<Panel class="NodeIcon Shadow" hittest="false" />
							<Panel class="NodeIcon" hittest="false" />
						</Panel>
						<Label class="MinigameLabel" text="#DOTA_Crownfall_Archive_Minigame_Fishing" />
						<Label class="FishingActLabel LabelSmall" text="#DOTA_Crownfall_Archive_Act1" />
						<Panel class="FishingButtonBorder" hittest="false" />
					</Panel>
					<Panel class="FishingButton" onactivate="DOTACrownfallMinigameOasisFishing()">
						<Image class="FishingMinigameThumbnailImage" src="file://{images}/events/crownfall/archive/oasis_fishing.png" scaling="stretch-to-fit-preserve-aspect"></Image>	
						<Panel class="Node MainQuestNode EncounterNode fishing" hittest="false">
							<Panel id="NodeBackground" />
							<Panel id="NodeBackgroundInner" hittest="false" />
							<Panel class="NodeIcon Shadow" hittest="false" />
							<Panel class="NodeIcon" hittest="false" />
						</Panel>
						<Label class="MinigameLabel" text="#DOTA_Crownfall_Archive_Minigame_OasisFishing" />
						<Label class="FishingActLabel LabelSmall" text="#DOTA_Crownfall_Archive_Act2" />
						<Panel class="FishingButtonBorder" hittest="false" />
					</Panel>
					<Panel class="FishingButton" onactivate="DOTACrownfallMinigameSandFishing()">
						<Image class="FishingMinigameThumbnailImage" src="file://{images}/events/crownfall/archive/sand_fishing.png" scaling="stretch-to-fit-preserve-aspect"></Image>	
						<Panel class="Node MainQuestNode EncounterNode fishing" hittest="false">
							<Panel id="NodeBackground" />
							<Panel id="NodeBackgroundInner" hittest="false" />
							<Panel class="NodeIcon Shadow" hittest="false" />
							<Panel class="NodeIcon" hittest="false" />
						</Panel>
						<Label class="MinigameLabel" text="#DOTA_Crownfall_Archive_Minigame_SandFishing" />
						<Label class="FishingActLabel LabelSmall" text="#DOTA_Crownfall_Archive_Act3" />
						<Panel class="FishingButtonBorder" hittest="false" />
					</Panel>
				</Panel>
			</Panel>
		</Panel>
	</DOTABPCrownfallArchivePage>
</root>
