<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/battle_pass/crownfall/bp_crownfall_shared.css" />
		<include src="file://{resources}/styles/battle_pass/crownfall/bp_tooltip_crownfall_store_pack_sticker.css" />
	</styles>

	<Panel class="TooltipContents">
		<Panel class="Quest">
			<Panel class="QuestHeader">
				<Panel class="QuestIcon" onmouseover="UIShowTextTooltip( '#DOTA_CrownfallStore_ViewOnMap' )" onmouseout="UIHideTextTooltip()">
					<Image class="MapIcon" src="file://{images}/control_icons/24px/location.svg" />
				</Panel>
				<Panel class="QuestHeaderDetails">
					<Label class="QuestLabel" text="#DOTA_Overworld_Node_Encounter" />
					<Label class="QuestTitle" text="#DOTA_Crownfall_EncounterName_Act4_Memorabilia" />
				</Panel>
			</Panel>
			<Label class="EncounterDetails" text="#DOTA_Crownfall_Encounter_StickerVendor_Explanation" />
		</Panel>
	</Panel>
</root>