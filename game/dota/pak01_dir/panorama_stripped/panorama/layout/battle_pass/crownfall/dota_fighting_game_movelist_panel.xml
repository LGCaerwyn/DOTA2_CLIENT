<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/popups/popups_shared.css" />
		<include src="file://{resources}/styles/battle_pass/crownfall/bp_crownfall_shared.css" />
		<include src="file://{resources}/styles/battle_pass/crownfall/bp_popup_crownfall_encounter_fighting.css" />
	</styles>

	<snippets>
		<snippet name="MoveListEntry" >
			<Panel class="MoveListEntry" >
				<Label class="Name" text="{s:move_name}" />
				<Panel class="Command" id="command">
					                                           
				</Panel>
				<Label class="Startup" text="{s:startup}" />
				<Label class="OnBlock" text="{s:on_block}" />
				<Label class="OnHit" text="{s:on_hit}" />
				<Label class="Damage" text="{s:damage}" />
			</Panel>
		</snippet>
		<snippet name="MoveListInstallHeader" >
			<Panel class="MoveListInstallHeader" >
				<Label class="InstallRequirement" text="#DOTA_FightingGame_InstallRequirementHeader" />
			</Panel>
		</snippet>
	</snippets>
	
	<DOTAFightingGameMoveListPanel>
		<Label id="MoveListTitle" html="true" text="#DOTA_FightingGame_MovelistTitle"/>
		<Panel class="MoveList">
			<Panel class="MoveListHeader">
				<Panel class="FillWidth"/>
				<Label class="Command" text="#DOTA_FightingGame_Movelist_Input" />
				<Label class="Startup" text="#DOTA_FightingGame_Movelist_Startup" />
				<Label class="OnBlock" text="#DOTA_FightingGame_Movelist_OnBlock" />
				<Label class="OnHit" text="#DOTA_FightingGame_Movelist_OnHit" />
				<Label class="Damage" text="#DOTA_FightingGame_Movelist_Damage" />
			</Panel>
			<Panel id="MoveListContainer">
				                                                   
			</Panel>
		</Panel>
		<Panel class="PanelOutline" hittest="false"/>
	</DOTAFightingGameMoveListPanel>
</root>
