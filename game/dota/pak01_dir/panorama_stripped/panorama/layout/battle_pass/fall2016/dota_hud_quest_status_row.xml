<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/battle_pass/fall2016/bp_fall2016.css" />
		
		<include src="file://{resources}/styles/battle_pass/quest_ranks.css" />
		<include src="file://{resources}/styles/hud/dota_hud_plus_status.css" />
	</styles>

	<DOTAQuestStatusRow hittest="false">
		<Panel class="QuestListItem" hittest="false">
			<Panel class="QuestStatusDesc LeftRightFlow" hittest="false">
				<Panel id="QuestStars" class="LeftRightFlow">
					<Panel class="Star Rank1"/>
					<Panel class="Star Rank2"/>
					<Panel class="Star Rank3"/>
				</Panel>
				<Label id="Progress" class="ProgressNumbers" text="{d:progress} / {d:goal}" hittest="false"/>
				<Label id="QuestDesc" html="true" hittest="false"/>
			</Panel>
		</Panel>
	</DOTAQuestStatusRow>
</root>