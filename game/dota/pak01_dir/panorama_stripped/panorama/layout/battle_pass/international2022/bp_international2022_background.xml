<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/dashboard_page_shared.css" />
		<include src="file://{resources}/styles/battle_pass/international2022/bp_international2022_shared_part2.css" />
		<include src="file://{resources}/styles/battle_pass/international2022/bp_international2022_background.css" />
	</styles>

	<DOTABPBackgroundInternational2022>

		<Panel id="BattlePassBackgroundImage" hittest="false" />

		                                                                                                                                                                                                                                                                                            
		                                                                                                                                                                                                                                                                                           
		<Panel class="BackgroundFXContainerPart2" hittest="false">
			<DOTAParticleScenePanel id="backgroundfx" class="BackgroundFXDiretide" particleName="particles/ui/battle_pass/ui_bp_2022_dashboard_diretide_background.vpcf"
				renderdeferred="false" hittest="false" particleonly="false" startActive="true" cameraOrigin="200 0 0" lookAt="0 0 0" fov="60" squarePixels="true" />
		</Panel>
		<Panel class="FooterGradient" hittest="false" />


		
		                                                          
			                                                                                                                                                                                                                                            

			                                                                                                                                  
				                                                                                                                                                    

			                                                                                                                       
				                                                                                                                                                   
		
			                                                                                                                    
				                                                                                                                                                    
			                                                       
			                                                                                                                                                                      
		            


		<Panel class="BackgroundLogoContainerDiretide">
			<Panel class="DireTide2022Logo" onactivate="DOTAShowBPInternational2022HomePage()"/>
		</Panel>
				<DOTABPInternational2022EventSummary id="EventSummary"/>
			<DOTAParticleScenePanel id="foregroundfxblur" class="BackgroundFXDiretideForegroundBlur" particleName="particles/ui/battle_pass/ui_bp_2022_dashboard_diretide_foreground.vpcf"
				renderdeferred="false" hittest="false" particleonly="false" startActive="true" cameraOrigin="30 0 0" lookAt="0 0 0" fov="60" squarePixels="true" />
			<DOTAParticleScenePanel id="foregroundfxscreenleft" class="BackgroundFXDiretideForegroundScreenLeft" particleName="particles/ui/battle_pass/ui_bp_2022_dashboard_diretide_foreground_screen.vpcf"
				renderdeferred="false" hittest="false" particleonly="false" startActive="true" cameraOrigin="100 20 5" lookAt="0 20 5" fov="20" squarePixels="true" />
				<DOTAParticleScenePanel id="foregroundfxscreenright" class="BackgroundFXDiretideForegroundScreenRight" particleName="particles/ui/battle_pass/ui_bp_2022_dashboard_diretide_foreground_screen.vpcf"
				renderdeferred="false" hittest="false" particleonly="false" startActive="true" cameraOrigin="-100 20 5" lookAt="0 20 5" fov="20" squarePixels="true" />
		<Panel id="BPHomeSlantedBG"/>

	</DOTABPBackgroundInternational2022>
 

</root>

