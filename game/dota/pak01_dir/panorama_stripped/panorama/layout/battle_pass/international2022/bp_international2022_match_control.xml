<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/frontpage_shared.css" />
		<include src="file://{resources}/styles/leaf_pages/dashboard_page_debut_styles.css" />	
		<include src="file://{resources}/styles/dpc/widgets/game_details.css" />
		<include src="file://{resources}/styles/battle_pass/international2022/bp_international2022_shared_part2.css" />
		<include src="file://{resources}/styles/battle_pass/international2022/bp_international2022_takeover.css" />
	</styles>

	<DOTAFrontpageInternational2022MatchControl>
		<Panel id="videoSelectedVerticalBar" />	
		<Panel id="ControlContainer" >
			<Panel id="radiant_pip_1" class="GDC_Summary_Header_Game GDCPip GDCPip__Radiant" />
			<Panel id="radiant_pip_2" class="GDC_Summary_Header_Game GDCPip GDCPip__Radiant" />
			<Panel id="radiant_pip_3" class="GDC_Summary_Header_Game GDCPip GDCPip__Radiant" />
			<DOTATeamImage id="Team1" class="ControlTeam" teamimagestyle="squaremedium" onactivate="" hittest="false"/>
			<Panel id="VsTimerCol" >
				<Panel id="playVideoIcon" />
				<Label id="VSLabel" text="#DOTA_International2022_vs" />
				<DOTACountdown id="match_countdown" server-time="true" >
					<Label id="match_timer" class="time-left" text="{t:d:t:countdown_time}" />
				</DOTACountdown>
			</Panel>
			<DOTATeamImage id="Team2" class="ControlTeam" teamimagestyle="squaremedium" onactivate="" hittest="false"/>
			<Panel id="dire_pip_3" class="GDC_Summary_Header_Game GDCPip GDCPip__Dire" />
			<Panel id="dire_pip_2" class="GDC_Summary_Header_Game GDCPip GDCPip__Dire" />
			<Panel id="dire_pip_1" class="GDC_Summary_Header_Game GDCPip GDCPip__Dire" />
		</Panel>
	</DOTAFrontpageInternational2022MatchControl>

</root>
