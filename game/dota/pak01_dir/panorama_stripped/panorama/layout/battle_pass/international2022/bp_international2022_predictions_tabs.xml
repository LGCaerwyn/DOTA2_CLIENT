<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/dashboard.css" />
		<include src="file://{resources}/styles/battle_pass/international2020/bp_international2020_shared.css" />
		<include src="file://{resources}/styles/battle_pass/international2020/comp_ti10_tabs.css" />
	</styles>

	<Panel class="SecondaryTabSet">

		<RadioButton id="Home" class="SecondaryTabButton" group="SecondaryTabGroup" text="#DOTA_SeasonPass_SecondaryTabHeader_Home" onactivate="DOTAShowBPInternational2022HomePage()" />

		<Label class="SecondaryTabSeparator" text="/" />

		<RadioButton id="Talent" class="SecondaryTabButton" group="SecondaryTabGroup" text="#DOTA_Compendium_SecondaryTabHeader_Casters" onactivate="DOTAShowCompTI10CastersPage()" />

		<Label class="SecondaryTabSeparator" text="/" />

		<RadioButton id="PlayerCards" class="SecondaryTabButton" group="SecondaryTabGroup" text="#DOTA_Compendium_SecondaryTabHeader_PlayerCards" onactivate="DOTAShowCompTI10PlayerCardsPage()" />

		<Label class="SecondaryTabSeparator" text="/" />

		<RadioButton id="Fantasy" class="SecondaryTabButton" group="SecondaryTabGroup" text="#DOTA_Compendium_Fantasy" onactivate="DOTAShowCompTI10FantasyPage(0)" disabled="false" />

		<Label class="SecondaryTabSeparator" text="/" />

		<RadioButton id="MainEventPredictions" class="SecondaryTabButton" group="SecondaryTabGroup" text="#DOTA_Compendium_MainEventPredictions" onactivate="DOTAShowBPInternational2022MainEventPredictionsPage(1)" disabled="false" />
		
		<Label class="SecondaryTabSeparator" text="/" />

		<RadioButton id="BracketPredictions" class="SecondaryTabButton" group="SecondaryTabGroup" text="#DOTA_Compendium_BracketPredictions" onactivate="DOTAShowCompDoubleElimBracketPage( EVENT_ID_INTERNATIONAL_2022 )" disabled="false" />

	</Panel>
	
</root>
