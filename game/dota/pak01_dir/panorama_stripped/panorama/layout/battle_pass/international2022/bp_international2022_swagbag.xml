<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/dashboard_page_shared.css" />
		<include src="file://{resources}/styles/battle_pass/international2022/bp_international2022_shared_part2.css" />
		<include src="file://{resources}/styles/battle_pass/international2022/bp_international2022_swagbag_summary.css" />
		<include src="file://{resources}/styles/battle_pass/international2022/bp_international2022_swagbag.css" />
	</styles>

	<DOTABPInternational2022SwagBagPage class="DashboardPage FullDashboardPage" dashboardclass="SeasonalBattlePassPage International2022BattlePassPage International2022BPHome">
		<Panel class="BottomGradient" />
		<Panel id="SwagBagArt" />
		<Panel class="Content">
			<Panel class="Info">
				<Label class="SwagBagTitle HeaderText" text="#DOTA_International2022_Swagbag_SectionTitle" />
				<Label class="SwagBagDescription" text="#DOTA_International2022_Swagbag_SectionDescription" />
				<Panel class="SBSummary_ProgressContainer">
					<Panel class="SBSummary_Progress">
						<ProgressBar class="SBSummary_ProgressBar" id="ProgressBar" />
						<Label class="SBSummary_ProgressCount" text="#DOTA_International2022_Swagbag_ProgressBarLabel" />
					</Panel>
					<Label class="SBSummary_ProgressReason" text="#DOTA_International2022_Swagbag_LockedReason" />
				</Panel>
			</Panel>

			<Panel class="RewardContainer">
				<Panel id="Reward_0" class="RewardCell" >					
					<DOTAScenePanel hittest="false" class="IconParticles HorizontalCenter" map="scenes/item_received" camera="shot_camera" particleonly="true"/>
					<Panel class="RewardCellContent">		
						<Panel class="RewardIcon" />
						<Label class="RewardHeaderText" text="#DOTA_International2022_Swagbag_RewardLabel1" />
						<Label class="RewardTitle" text="#DOTA_International2022_Swagbag_RewardTitle1" />
						<Label class="RewardDescription" text="#DOTA_International2022_Swagbag_RewardDescription1" />
					</Panel>
					<TextButton class="LockedButton BattlePass2022ButtonSecondary Disabled" text="#DOTA_International2022_Swagbag_LockedLabel" />
					<TextButton class="ClaimButton BattlePass2022ButtonPrimary" text="#DOTA_International2022_Swagbag_ClaimButtonLabel" onactivate="DotaBPInternational2022ClaimSwagBagItem( 0 )" />
					<Label class="RewardClaimedLabel" text="#DOTA_International2022_Swagbag_ClaimedLabel" />
					<Panel class="LockedInfoIcon" onmouseover="UIShowTextTooltip( '#DOTA_International2022_Swagbag_LockedReasonTemporary' )" onmouseout="UIHideTextTooltip()" />
				</Panel>
				<Panel id="Reward_1" class="RewardCell" >					
					<DOTAScenePanel hittest="false" class="IconParticles HorizontalCenter" map="scenes/item_received" camera="shot_camera" particleonly="true"/>
					<Panel class="RewardCellContent">
						<Panel class="RewardIcon" />
						<Label class="RewardHeaderText" text="#DOTA_International2022_Swagbag_RewardLabel2" />
						<Label class="RewardTitle" text="#DOTA_International2022_Swagbag_RewardTitle2" />
						<Label class="RewardDescription" text="#DOTA_International2022_Swagbag_RewardDescription2" />
					</Panel>
					<TextButton class="LockedButton BattlePass2022ButtonSecondary Disabled" text="#DOTA_International2022_Swagbag_LockedLabel" />
					<TextButton class="ClaimButton BattlePass2022ButtonPrimary" text="#DOTA_International2022_Swagbag_ClaimButtonLabel" onactivate="DotaBPInternational2022ClaimSwagBagItem( 1 )" />
					<Label class="RewardClaimedLabel" text="#DOTA_International2022_Swagbag_ClaimedLabel" />
					<Panel class="LockedInfoIcon" onmouseover="UIShowTextTooltip( '#DOTA_International2022_Swagbag_LockedReasonTemporary' )" onmouseout="UIHideTextTooltip()" />
				</Panel>
				<Panel id="Reward_2" class="RewardCell" >					
					<DOTAScenePanel hittest="false" class="IconParticles HorizontalCenter" map="scenes/item_received" camera="shot_camera" particleonly="true"/>
					<Panel class="RewardCellContent">						
						<Panel class="RewardIcon" />
						<Label class="RewardHeaderText" text="#DOTA_International2022_Swagbag_RewardLabel3" />
						<Label class="RewardTitle" text="#DOTA_International2022_Swagbag_RewardTitle3" />
						<Label class="RewardDescription" text="#DOTA_International2022_Swagbag_RewardDescription3" />
					</Panel>
					<TextButton class="ClaimButton BattlePass2022ButtonPrimary" text="#DOTA_International2022_Swagbag_SelectArcanaButtonLabel" onactivate="DotaBPInternational2022ClaimSwagBagItem( 2 )" />
					<Label class="RewardClaimedLabel" text="#DOTA_International2022_Swagbag_ClaimedLabel" />
				</Panel>
			</Panel>
		</Panel>
	</DOTABPInternational2022SwagBagPage>
</root>
