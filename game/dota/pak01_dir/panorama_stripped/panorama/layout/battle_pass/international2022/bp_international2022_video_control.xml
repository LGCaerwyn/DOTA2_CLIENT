	</styles>

	<DOTAFrontpageInternational2022VideoControl>
		<Panel id="videoActiveVerticalBar" />
		<Panel id="titleHolder" >
			<Label class="controlHeader" text="{s:controlheader}" />
			<Label class="controlSubHeader" text="{s:controlsubheader}" />
		</Panel>
		<Label id="availableNow" text="#DOTA_International2022_TrueSightWeekAvailableNow" />
		<Panel id="dateComingSoonPanel" >
			<Label class="datePlaceholder" text="{s:date_time}" />
			<Label class="datePlaceholder coming" text="#DOTA_International2022_TrueSightWeekComing" />
		</Panel>
		<Panel id="playVideoIcon" />
	</DOTAFrontpageInternational2022VideoControl>

</root>
