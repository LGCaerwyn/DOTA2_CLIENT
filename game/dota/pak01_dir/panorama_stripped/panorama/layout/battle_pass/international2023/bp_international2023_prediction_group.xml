<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/dashboard_page_shared.css" />
		<include src="file://{resources}/styles/battle_pass/international2023/bp_international2023_shared.css" />
		<include src="file://{resources}/styles/battle_pass/international2023/bp_international2023_predictions.css" />
	</styles>

	<International2023PredictionGroup>
		<Panel class="GroupTeamSlot" id="Slot0" onactivate="DOTATeamPredictionGroupPopup(0)">
			<Label class="TeamRank" text="#DOTA_OraclesChallenge_1st" />
			<Panel class="GroupTeam">
				<Label class="NoTeamMark" text="{s:no_team}" />
				<DOTATeamImage teamid="0" id="TeamImage" teamimagestyle="squaresmall" />
				<Label class="TeamName" id="TeamName" text="{g:dota_team_name:t:team_name}" />
			</Panel>
			<Panel class="ResultIcon"/>
		</Panel>
