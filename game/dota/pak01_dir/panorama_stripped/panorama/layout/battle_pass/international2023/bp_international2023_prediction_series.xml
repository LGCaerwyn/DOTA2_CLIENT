<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/battle_pass/international2023/bp_international2023_shared.css" />
		<include src="file://{resources}/styles/battle_pass/international2023/bp_international2023_predictions.css" />
	</styles>

	<International2023PredictionSeries>
		<Panel class="GroupTeamRow">
			<Panel class="GroupTeamSlot" id="Slot0" >
				<Panel class="GroupTeam" >
					<Label class="NoTeamMark" text="{s:no_team}" />
					<DOTATeamImage teamid="0" id="TeamImage" teamimagestyle="squaresmall" />
					<Label class="TeamName" id="TeamName" text="{g:dota_team_name:t:team_name}" />
				</Panel>
				<Panel class="ResultIcon"/>
			</Panel>
			<Panel class="Crown" id="Crown0" onactivate="DOTATeamPredictionSeriesWinnerSelected(0)" />
			<Panel class="CrownResultIcon" id="CrownResultIcon0" />
		</Panel>
		<Panel class="GroupTeamRow">
			<Panel class="GroupTeamSlot" id="Slot1" onactivate="DOTATeamPredictionSeriesPopup(1)">
				<Panel class="GroupTeam">
					<Label class="NoTeamMark" text="{s:no_team}" />
					<DOTATeamImage teamid="0" id="TeamImage" teamimagestyle="squaresmall" />
					<Label class="TeamName" id="TeamName" text="{g:dota_team_name:t:team_name}" />
				</Panel>
				<Panel class="ResultIcon"/>
			</Panel>
			<Panel class="Crown" id="Crown1" onactivate="DOTATeamPredictionSeriesWinnerSelected(1)" />
			<Panel class="CrownResultIcon" id="CrownResultIcon1" />
		</Panel>
	</International2023PredictionSeries>

</root>
