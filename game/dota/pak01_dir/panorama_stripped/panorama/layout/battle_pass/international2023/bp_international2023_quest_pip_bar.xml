<root>

	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/battle_pass/international2023/bp_international2023_shared.css" />
		<include src="file://{resources}/styles/battle_pass/international2023/bp_international2023_quests.css" />
		<include src="file://{resources}/styles/battle_pass/international2023/bp_international2023_quests_pip_bar.css" />
	</styles>

	<snippets>
		<snippet name="PipSnippet">
			<Button class="PipIcon" hittest="true">
				<Panel class="PipBackground"/>	
				<Panel class="PipStar"/>
			</Button>
		</snippet>
	</snippets>

	<DOTAQuestPipBar_International2023>
			<Panel class="PipCountLabelContainer">
				<Label class="PipCountLabel PipCount" text="{d:quests_completed}"/>
				<Label class="PipCountLabel PipLabelSlash" text="/"/>
				<Label class="PipCountLabel PipCountMax" text="{d:quests_count}"/>
			</Panel>
			<Panel id="StarCountPipProgressBar" class="PipProgressBar">
				                              
			</Panel>
			<Button id="StarClaimRewardButton" class="ClaimButton ButtonPrimary Large Platinum" onactivate="DOTAInternational2023QuestPipBarClaimReward()">
				<Label class="PipRewardClaimLabel" text="#DOTA_Claim"/>
			</Button>

	</DOTAQuestPipBar_International2023>


</root>
