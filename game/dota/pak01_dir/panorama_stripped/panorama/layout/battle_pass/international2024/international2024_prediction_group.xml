<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/dashboard_page_shared.css" />
		<include src="file://{resources}/styles/battle_pass/international2024/international2024_shared.css" />
		<include src="file://{resources}/styles/battle_pass/international2024/international2024_predictions.css" />
	</styles>

	<International2024PredictionGroup>
		<Panel class="GroupTeamSlot" id="Slot0">
			<Panel class="GroupTeam">
				<Label class="NoTeamMark" text="{s:no_team}" onactivate="DOTATeamPrediction2024GroupPopup(0)" />
				<DOTATeamImage teamid="0" id="TeamImage" teamimagestyle="squaresmall" />
				<Panel class="ResultIcon"/>
				<Label class="TeamName" id="TeamName" text="{g:dota_team_name:t:team_name}" onactivate="DOTATeamPrediction2024GroupPopup(0)" onmouseover="UIShowTextTooltip( #DOTA_Edit )" onmouseout="UIHideTextTooltip()" />
			</Panel>
			<Label class="TeamRank" text="#DOTA_OraclesChallenge_1st" />
			<Image class="LockIcon" src="file://{images}/control_icons/24px/lock.svg" scaling="stretch-to-fit-preserve-aspect" />
		</Panel>
		<Panel class="GroupTeamSlot" id="Slot1">
			<Panel class="GroupTeam">
				<Label class="NoTeamMark" text="{s:no_team}" onactivate="DOTATeamPrediction2024GroupPopup(1)" />
				<DOTATeamImage teamid="0" id="TeamImage" teamimagestyle="squaresmall" />
				<Panel class="ResultIcon"/>
				<Label class="TeamName" id="TeamName" text="{g:dota_team_name:t:team_name}" onactivate="DOTATeamPrediction2024GroupPopup(1)" onmouseover="UIShowTextTooltip( #DOTA_Edit )" onmouseout="UIHideTextTooltip()" />
			</Panel>
			<Label class="TeamRank" text="#DOTA_OraclesChallenge_2nd" />
			<Image class="LockIcon" src="file://{images}/control_icons/24px/lock.svg" scaling="stretch-to-fit-preserve-aspect" />
		</Panel>
		<Panel class="GroupTeamSlot" id="Slot2">
			<Panel class="GroupTeam">
				<Label class="NoTeamMark" text="{s:no_team}" onactivate="DOTATeamPrediction2024GroupPopup(2)" />
				<DOTATeamImage teamid="0" id="TeamImage" teamimagestyle="squaresmall" />
				<Panel class="ResultIcon"/>
				<Label class="TeamName" id="TeamName" text="{g:dota_team_name:t:team_name}" onactivate="DOTATeamPrediction2024GroupPopup(2)" onmouseover="UIShowTextTooltip( #DOTA_Edit )" onmouseout="UIHideTextTooltip()" />
			</Panel>
			<Label class="TeamRank" text="#DOTA_OraclesChallenge_3rd" />
			<Image class="LockIcon" src="file://{images}/control_icons/24px/lock.svg" scaling="stretch-to-fit-preserve-aspect" />
		</Panel>
		<Panel class="GroupTeamSlot" id="Slot3">
			<Panel class="GroupTeam">
				<Label class="NoTeamMark" text="{s:no_team}" onactivate="DOTATeamPrediction2024GroupPopup(3)" />
				<DOTATeamImage teamid="0" id="TeamImage" teamimagestyle="squaresmall" />
				<Panel class="ResultIcon"/>
				<Label class="TeamName" id="TeamName" text="{g:dota_team_name:t:team_name}" onactivate="DOTATeamPrediction2024GroupPopup(3)" onmouseover="UIShowTextTooltip( #DOTA_Edit )" onmouseout="UIHideTextTooltip()" />
			</Panel>
			<Label class="TeamRank" text="#DOTA_OraclesChallenge_4th" />
			<Image class="LockIcon" src="file://{images}/control_icons/24px/lock.svg" scaling="stretch-to-fit-preserve-aspect" />
		</Panel>
	</International2024PredictionGroup>
	
</root>
