<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/battle_pass/international2024/international2024_shared.css" />
		<include src="file://{resources}/styles/battle_pass/international2024/international2024_predictions.css" />
	</styles>

	<International2024PredictionSeries>
		<Panel class="GroupTeamRow Team1">
			<Panel class="GroupTeamSlot" id="Slot0" >
				<Panel class="GroupTeam" >
					<DOTATeamImage teamid="0" id="TeamImage" teamimagestyle="squaresmall" />
					<Panel class="ResultIcon"/>
					<Label class="TeamName" id="TeamName" text="{g:dota_team_name:t:team_name}" />
					<Label class="NoTeamMark" text="{s:no_team}" onactivate="DOTATeamPrediction2024SeriesPopup(0)" />
				</Panel>
				<Panel class="Crown" id="Crown0" onactivate="DOTATeamPrediction2024SeriesWinnerSelected(0)" onmouseover="UIShowTextTooltip( #DOTA_OraclesChallenge_SelectWinner )" onmouseout="UIHideTextTooltip()" />
			</Panel>
			<Panel class="CrownResultIcon" id="CrownResultIcon0" />
			<Image class="LockIcon" src="file://{images}/control_icons/24px/lock.svg" scaling="stretch-to-fit-preserve-aspect" />
		</Panel>
		<Panel class="GroupTeamRow Team2">
			<Panel class="GroupTeamSlot" id="Slot1">
				<Panel class="GroupTeam">
					<DOTATeamImage teamid="0" id="TeamImage" teamimagestyle="squaresmall" />
					<Panel class="ResultIcon"/>
					<Label class="TeamName" id="TeamName" text="{g:dota_team_name:t:team_name}" onactivate="DOTATeamPrediction2024SeriesPopup(1)" onmouseover="UIShowTextTooltip( #DOTA_Edit )" onmouseout="UIHideTextTooltip()" />
					<Label class="NoTeamMark" text="{s:no_team}" onactivate="DOTATeamPrediction2024SeriesPopup(1)" />
				</Panel>
				<Panel class="Crown" id="Crown1" onactivate="DOTATeamPrediction2024SeriesWinnerSelected(1)" onmouseover="UIShowTextTooltip( #DOTA_OraclesChallenge_SelectWinner )" onmouseout="UIHideTextTooltip()" />
			</Panel>
			<Panel class="CrownResultIcon" id="CrownResultIcon1" />
			<Image class="LockIcon" src="file://{images}/control_icons/24px/lock.svg" scaling="stretch-to-fit-preserve-aspect" />
		</Panel>
	</International2024PredictionSeries>

</root>
