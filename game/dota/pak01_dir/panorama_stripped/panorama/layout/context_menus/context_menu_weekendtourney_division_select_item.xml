<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/context_menus/context_menu_weekendtourney_division_select_item.css" />
	</styles>

	<DOTAContextMenuWeekendTourneyDivisionSelectItem>
		<RadioButton id="RadioButton" group="WeekendTourneyDivisions" onactivate="DOTAWeekendTourneyDivisionSelectRadioClicked();">
			<Panel id="RadioButtonBody">
				<Label id="DivisionName" text="{s:division_name}" />
				<Label id="NoTournamentScheduledLabel" class="ScheduleStateLabel" text="#WeekendTourneyDivisionStatus_NoTournamentScheduled"/>
				<Label id="StartingTimeLongLabel" class="ScheduleStateLabel" text="#WeekendTourneyDivisionStatus_StartingTimeLong"/>
				<Label id="StartingTimeShortLabel" class="ScheduleStateLabel" text="#WeekendTourneyDivisionStatus_StartingTimeShort"/>
				<Label id="NowOpenLabel" class="ScheduleStateLabel" text="#WeekendTourneyDivisionStatus_IsNowOpen"/>
				<Label id="TooLateLabel" class="ScheduleStateLabel" text="#WeekendTourneyDivisionStatus_TooLate"/>
				<Panel id="DivisionRegionsRow">
					            
						                                                                                                 
						   
					   
				</Panel>
			</Panel>
		</RadioButton>
	</DOTAContextMenuWeekendTourneyDivisionSelectItem>
</root>
