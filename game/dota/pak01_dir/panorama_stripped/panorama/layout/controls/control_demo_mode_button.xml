<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/battle_pass/current_battle_pass.css" />
		<include src="file://{resources}/styles/controls/control_demo_mode_button.css" />
	</styles>

	<DemoModeButton>

		                                             
		                                                                                                                                                                                        
			                                                          
				                                                                  
				                           
			        
		         
		                                                                                                                                                                                            
			                                                          
				                                                                         
				                           
			        
		             

		<Button class="DemoButton StartDemo ButtonPrimary" onactivate="DOTADemoButtonClicked( false )" onmouseover="UIShowTextTooltip( #DOTA_HeroDemo_Button_Tooltip )" onmouseout="UIHideTextTooltip()">
			<Label class="" text="#DOTA_HeroDemo_Button" />
			<Image src="file://{images}/control_icons/24px/arrow_top_right.svg" />
		</Button>
		<Button class="DemoButton ReturnToDemo ButtonPrimary" onactivate="DOTADemoButtonClicked( true )" onmouseover="UIShowTextTooltip( #DOTA_HeroDemo_Button_Return_Tooltip )" onmouseout="UIHideTextTooltip()">
			<Label class="" text="#DOTA_HeroDemo_Button_Return" />
			<Image src="file://{images}/control_icons/24px/arrow_top_right.svg" />
		</Button>
	</DemoModeButton>
</root>
