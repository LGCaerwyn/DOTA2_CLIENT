<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
        <include src="file://{resources}/styles/frontpage_shared.css" />
        <include src="file://{resources}/styles/leaf_pages/dashboard_page_debut_styles.css" />
	    <include src="file://{resources}/styles/dashboard_frontpage_10th_anniversary_event.css" />
        <include src="file://{resources}/styles/dota_ui_10th_anniversary_treasure_details.css" />
	</styles>

	<scripts>
		<include src="file://{resources}/scripts/util.ts" />
		<include src="file://{resources}/scripts/sequence_actions.ts" />
		<include src="file://{resources}/scripts/dota_sequence_actions.ts" />
		<include src="file://{resources}/scripts/dashboard_background_10th_anniversary.ts" />

	</scripts>

    <Panel class="BackgroundPanel" >
		<DOTAScenePanel id="BackgroundScene" onload="RunPageAnimation()" map="maps/backgrounds/dashboard_background_10th_anniversary" light="hero_light" camera="hero_camera" particleonly="false" renderdeferred="true" />
      	<Panel id="BGVignette" hittest="false" />
        <Panel id="DashboardTitleContainer">
		<Panel class="TreasureOverview TopBottomFlow">
			<Panel class="Big10Container">
				<DOTAScenePanel class="Big10ParticlesTop" id="" map="maps/backgrounds/dashboard_background_10th_anniversary_logoBG" renderdeferred="false" hittest="false" particleonly="true" />
				<Panel class="Big10Emblem"></Panel>

				<Label class="Big10Label" text="#DOTA_10thAnniversary_MainLogo_10" />
				<Label class="Big10Label" text="#DOTA_10thAnniversary_MainLogo_10" />
				<DOTAScenePanel class="Big10ParticlesTop" id="" map="maps/backgrounds/dashboard_background_10th_anniversary_logo" renderdeferred="false" hittest="false" particleonly="true" />

			</Panel>
			<Panel class="AnniversaryContainer">
				<Panel class="YearsContainer">
					<Label class="YearsLabel LabelMedium" text="#DOTA_10thAnniversary_MainLogo_YearLabel" />
					<Panel class="Dash"/>
				</Panel>
				<Label class="AnniversaryLabel" text="#DOTA_10thAnniversary_MainLogo_AnniversaryLabel" />
				<Panel class="TreasureContainer">
					<Panel class="Dash"/>
					<Label class="TreasureLabel LabelLarge" text="#DOTA_10thAnniversary_MainLogo_TreasureLabel" />
				</Panel>		
				<Label class="TreasureDescription" text="#DOTA_10thAnniversary_Description" />
			</Panel>			
		</Panel>
        </Panel>

	</Panel>

    

</root>