<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/dashboard_background_collectors_cache_2024_2.css" />
	</styles>

	<scripts>
		<include src="file://{resources}/scripts/dashboard_background_loadout_cc_2024.ts" />
	</scripts>

    <Panel class="CollectorsCache2024Background" >
		<Panel class="BackgroundImage" />
		<Image class="BackgroundImageGlow" src="file://{images}/backgrounds/cc2024_dashboard_backround_glow.png" scaling="stretch-to-fit-preserve-aspect" />
		<Panel class="HeroesSceneModels">
			<DOTAUIEconSetPreview id="PremiumReward_Undying" rotation="-60" itemdef="28276" drawbackground="false" displaymode="loadout_small" require-composition-layer="true" suppress-intro-effects="true" />
			<DOTAUIEconSetPreview id="PremiumReward_Invoker" rotation="15" itemdef="30575" drawbackground="false" displaymode="loadout_small" require-composition-layer="true" suppress-intro-effects="true" />
			<DOTAUIEconSetPreview id="PremiumReward_Rubick" rotation="-24" itemdef="29096" drawbackground="false" activity-modifier="front_page" displaymode="loadout_small" require-composition-layer="true" suppress-intro-effects="true" />
		</Panel>
		<Panel class="BGVignette" hittest="false" />
		<DOTAParticleScenePanel class="SmokeFX" hittest="false" particleName="particles/ui/battle_pass/ui_bp_2022_diretide_home_background.vpcf" require-composition-layer="true" renderdeferred="true" particleonly="false" startActive="true" cameraOrigin="200 0 0" lookAt="0 0 0" fov="60" squarePixels="true" />
		<DOTAScenePanel id="FxScene" class="TitleParticles LogoTrigger" onload="LogoTrigger()" hittest="false" map="maps/ui/dashboard_cc_logo" particleonly="false" />
		<Panel class="TitleContainer">
			<Image class="CrownfallLogo en" src="file://{images}/events/crownfall/logos/crownfall_logo_en.psd" scaling="stretch-to-fit-preserve-aspect" />
			<Image class="CrownfallLogo cn" src="file://{images}/events/crownfall/logos/crownfall_logo_cn.psd" scaling="stretch-to-fit-preserve-aspect" />
			<Label class="CCTitle TitleExtraLarge" text="#DOTA_CollectorsCache_Title_2"/>
			<Label class="CCAvailable BodyMedium" text="#DOTA_Collectors_2024_Cell_Description"/>
			<Panel class="ButtonContainer">
				<Panel class="Dash Left"/>
				<Image class="Emblem Left" src="file://{images}/events/crownfall/emblems/emblem_edge.png" scaling="stretch-to-fit-preserve-aspect" />
				<DOTACrownfallButton id="BuyNowButton" text="#DOTA_Treasure_ViewTreasure" class="" onactivate="DOTAShowTreasurePage( 30279 )" />
				<Image class="Emblem Right" src="file://{images}/events/crownfall/emblems/emblem_edge.png" scaling="stretch-to-fit-preserve-aspect" />
				<Panel class="Dash Right"/>
			</Panel>
		</Panel>
	</Panel>
</root>