<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/dashboard_background_map_update_2023.css" />
	</styles>

	<scripts>
		<include src="file://{resources}/scripts/util.ts" />
		<include src="file://{resources}/scripts/sequence_actions.ts" />
		<include src="file://{resources}/scripts/dota_sequence_actions.ts" />
		<include src="file://{resources}/scripts/dashboard_background_map_update_2023.ts" />
	</scripts>

	<Panel class="MapUpdateBackground">

		<Panel class="MapImage" />
		<Movie id="IntroMovie" src="file://{resources}/videos/events/map_update_2023/map_update_2023_embiggening.webm" repeat="false" controls="none" hittest="false" />
		
		<Panel class="MapUpdateVignette" />

		<Panel class="MainContents" hittest="false">

			<Panel id="MapFeatures">
				<Panel id="Watcher1" class="TooltipTarget Watcher1" mapfeature="Watcher" onmouseover="HighlightFeature( 'Watcher1' )" onmouseout="ClearHighlight()" />
				<Panel id="Watcher2" class="TooltipTarget Watcher2" mapfeature="Watcher" onmouseover="HighlightFeature( 'Watcher2' )" onmouseout="ClearHighlight()" />
				<Panel id="Watcher3" class="TooltipTarget Watcher3" mapfeature="Watcher" onmouseover="HighlightFeature( 'Watcher3' )" onmouseout="ClearHighlight()" />
				<Panel id="Watcher4" class="TooltipTarget Watcher4" mapfeature="Watcher" onmouseover="HighlightFeature( 'Watcher4' )" onmouseout="ClearHighlight()" />
				<Panel id="Watcher5" class="TooltipTarget Watcher5" mapfeature="Watcher" onmouseover="HighlightFeature( 'Watcher5' )" onmouseout="ClearHighlight()" />
				<Panel id="Watcher6" class="TooltipTarget Watcher6" mapfeature="Watcher" onmouseover="HighlightFeature( 'Watcher6' )" onmouseout="ClearHighlight()" />
				<Panel id="Watcher7" class="TooltipTarget Watcher7" mapfeature="Watcher" onmouseover="HighlightFeature( 'Watcher7' )" onmouseout="ClearHighlight()" />
				<Panel id="Watcher8" class="TooltipTarget Watcher8" mapfeature="Watcher" onmouseover="HighlightFeature( 'Watcher8' )" onmouseout="ClearHighlight()" />
				<Panel id="Sentinel1" class="TooltipTarget Sentinel1" mapfeature="Sentinel" onmouseover="HighlightFeature( 'Sentinel1' )" onmouseout="ClearHighlight()" />
				<Panel id="Sentinel2" class="TooltipTarget Sentinel2" mapfeature="Sentinel" onmouseover="HighlightFeature( 'Sentinel2' )" onmouseout="ClearHighlight()" />
				<Panel id="Roshan" class="TooltipTarget Roshan" mapfeature="Roshan" onmouseover="HighlightFeature( 'Roshan' )" onmouseout="ClearHighlight()" />
				<Panel id="RoshanDire" class="TooltipTarget RoshanDire" mapfeature="Roshan" onmouseover="HighlightFeature( 'RoshanDire' )" onmouseout="ClearHighlight()" />
				<Panel id="Tree1" class="TooltipTarget Tree1" mapfeature="Tree" onmouseover="HighlightFeature( 'Tree1' )" onmouseout="ClearHighlight()" />
				<Panel id="Tree2" class="TooltipTarget Tree2" mapfeature="Tree" onmouseover="HighlightFeature( 'Tree2' )" onmouseout="ClearHighlight()" />
				<Panel id="WisdomRune1" class="TooltipTarget WisdomRune1" mapfeature="WisdomRune" onmouseover="HighlightFeature( 'WisdomRune1' )" onmouseout="ClearHighlight()" />
				<Panel id="WisdomRune2" class="TooltipTarget WisdomRune2" mapfeature="WisdomRune" onmouseover="HighlightFeature( 'WisdomRune2' )" onmouseout="ClearHighlight()"/>
				                                                                                                                                             
				<Panel id="Portal1" class="TooltipTarget Portal1" mapfeature="Portal" onmouseover="HighlightFeature( 'Portal1' )" onmouseout="ClearHighlight()" />
				<Panel id="Portal2" class="TooltipTarget Portal2" mapfeature="Portal" onmouseover="HighlightFeature( 'Portal2' )" onmouseout="ClearHighlight()" />
				<Panel id="Outpost1" class="TooltipTarget Outpost1" mapfeature="Outpost" onmouseover="HighlightFeature( 'Outpost1' )" onmouseout="ClearHighlight()" />
				<Panel id="Outpost2" class="TooltipTarget Outpost2" mapfeature="Outpost" onmouseover="HighlightFeature( 'Outpost2' )" onmouseout="ClearHighlight()" />
				<Panel id="Outpost3" class="TooltipTarget Outpost3" mapfeature="Outpost" onmouseover="HighlightFeature( 'Outpost3' )" onmouseout="ClearHighlight()" />
				<Panel id="Outpost4" class="TooltipTarget Outpost4" mapfeature="Outpost" onmouseover="HighlightFeature( 'Outpost4' )" onmouseout="ClearHighlight()" />
				<Panel id="ShieldRune1" class="TooltipTarget ShieldRune1" mapfeature="ShieldRune" onmouseover="HighlightFeature( 'ShieldRune1' )" onmouseout="ClearHighlight()" />
				<Panel id="ShieldRune2" class="TooltipTarget ShieldRune2" mapfeature="ShieldRune" onmouseover="HighlightFeature( 'ShieldRune2' )" onmouseout="ClearHighlight()" />
				<Panel id="Gate1" class="TooltipTarget Gate1" mapfeature="Gate" onmouseover="HighlightFeature( 'Gate1' )" onmouseout="ClearHighlight()" />
				<Panel id="Gate2" class="TooltipTarget Gate2" mapfeature="Gate" onmouseover="HighlightFeature( 'Gate2' )" onmouseout="ClearHighlight()" />
				<Panel id="CreepCamp1" class="TooltipTarget CreepCamp1" mapfeature="Camp" onmouseover="HighlightFeature( 'CreepCamp1' )" onmouseout="ClearHighlight()" hittest="false" />
			</Panel>

			<Panel class="CenterContents" hittest="false">
				<Label class="Title" text="#FrontPage_MapUpdate2023_Title" hittest="false"/>
				<Label class="Subtitle" text="#FrontPage_MapUpdate2023_Subtitle" hittest="false" />
			</Panel>

			<Panel class="UpdateInfo" hittest="false">

				<Panel class="PatchNotesHeader" id="HeaderLabel">
					<Panel class="PatchNotesHeaderContents">
						<Label class="PatchNotesHeaderTitle" text="#FrontPage_MapUpdate2023_GameplayTitle" />
						<Panel class="LeftRightFlow HorizontalCenter">
							<Label class="PatchNotesHeaderCopy" text="#DOTA_FrontPage_International2019_Website" onactivate="ExternalBrowserGoToDOTAURLAnchor( newfrontiers )" />
							<Label class="PatchNotesHeaderCopy" text="#DOTA_PatchNotes" onactivate="ExternalBrowserGoToDOTAURLAnchor( newfrontiers, BalanceChanges )" />
						</Panel>
					</Panel>
				</Panel>

				<Panel class="UpdateSection" id="MapSection">
					<Panel class="BackgroundImage" />
					<Panel class="UpdateSectionContents">
						<Panel class="UpdateSectionHeader">
							<Label class="UpdateSectionTitle" text="#DOTA_MapUpdate2023_NewMap" />
						</Panel>
						<Panel id="MapFeatureNotes" class="UpdateSectionNav">
							<Label id="RoshanNote" mapfeature="Roshan" class="UpdateNoteNav" text="#DOTA_MapUpdate2023_NewMap_Roshan" onmouseover="HighlightFeature( 'RoshanNote' )" onmouseout="ClearHighlight()" />
							<Label id="TwinGatesNote" mapfeature="Portal" class="UpdateNoteNav" text="#DOTA_MapUpdate2023_NewMap_TwinGates" onmouseover="HighlightFeature( 'TwinGatesNote' )" onmouseout="ClearHighlight()" />
							<Label id="LotusPoolsNote" mapfeature="Tree" class="UpdateNoteNav" text="#DOTA_MapUpdate2023_NewMap_LotusPools" onmouseover="HighlightFeature( 'LotusPoolsNote' )" onmouseout="ClearHighlight()" />
							<Label id="TormentorsNote" mapfeature="Sentinel" class="UpdateNoteNav" text="#DOTA_MapUpdate2023_NewMap_Tormentors" onmouseover="HighlightFeature( 'TormentorsNote' )" onmouseout="ClearHighlight()" />
							<Label id="WatchersNote" mapfeature="Watcher" class="UpdateNoteNav" text="#DOTA_MapUpdate2023_NewMap_Watchers" onmouseover="HighlightFeature( 'WatchersNote' )" onmouseout="ClearHighlight()" />
							<Label id="BarriersNote" mapfeature="Gate" class="UpdateNoteNav" text="#DOTA_MapUpdate2023_NewMap_Barriers" onmouseover="HighlightFeature( 'BarriersNote' )" onmouseout="ClearHighlight()" />
							<Label id="WisdomRunesNote" mapfeature="WisdomRune" class="UpdateNoteNav" text="#DOTA_MapUpdate2023_NewMap_WisdomRunes" onmouseover="HighlightFeature( 'WisdomRunesNote' )" onmouseout="ClearHighlight()" />
							<Label id="ShieldRunesNote" mapfeature="ShieldRune" class="UpdateNoteNav" text="#DOTA_MapUpdate2023_NewMap_ShieldRunes" onmouseover="HighlightFeature( 'ShieldRunesNote' )" onmouseout="ClearHighlight()" />
							<Label id="CreepCampsNote" mapfeature="Camp" class="UpdateNoteNav" text="#DOTA_MapUpdate2023_NewMap_CreepCamps" onmouseover="HighlightFeature( 'CreepCampsNote' )" onmouseout="ClearHighlight()" />
							<Label id="OutpostsNote" mapfeature="Outpost" class="UpdateNoteNav" text="#DOTA_MapUpdate2023_NewMap_Outposts" onmouseover="HighlightFeature( 'OutpostsNote' )" onmouseout="ClearHighlight()" />
						</Panel>
					</Panel>
				</Panel>

				<Panel class="SectionsCarouselContainer">
					<Carousel id="SectionsCarousel" panels-visible="1" autoscroll-delay="5.0s,0.3s">
						<Panel class="UpdateSection" id="MajorGameplaySection" onactivate="ExternalBrowserGoToDOTAURLAnchor( newfrontiers, MajorGameplay )">
							<Panel class="BackgroundImage" />
							<Panel class="UpdateSectionContents">

								<Panel class="UpdateSectionHeader">
									<Label class="UpdateSectionTitle" text="#DOTA_MapUpdate2023_MajorGameplay" />
									                                   
								</Panel>
								<Panel class="UpdateSectionBody">
									<Label class="UpdateNote" text="#DOTA_MapUpdate2023_MajorGameplay_Desc" />

								</Panel>

							</Panel>
						</Panel>

						<Panel class="UpdateSection" id="MatchmakingSection" onactivate="ExternalBrowserGoToDOTAURLAnchor( newfrontiers, Matchmaking )">
							<Panel class="BackgroundImage" />
							<Panel class="UpdateSectionContents">

								<Panel class="UpdateSectionHeader">
									<Label class="UpdateSectionTitle" text="#DOTA_MapUpdate2023_Matchmaking" />
									                                   
								</Panel>
								<Panel class="UpdateSectionBody">
									<Label class="UpdateNote" text="#DOTA_MapUpdate2023_Matchmaking_Desc" />

								</Panel>
							</Panel>
						</Panel>

						<Panel class="UpdateSection" id="UserInterfaceSection" onactivate="ExternalBrowserGoToDOTAURLAnchor( newfrontiers, UserInterface )">
							<Panel class="BackgroundImage" />
							<Panel class="UpdateSectionContents">

								<Panel class="UpdateSectionHeader">
									<Label class="UpdateSectionTitle" text="#DOTA_MapUpdate2023_UserInterface" />
								</Panel>
								<Panel class="UpdateSectionBody">
									<Label class="UpdateNote" text="#DOTA_MapUpdate2023_UserInterface_Desc" />
								</Panel>
							</Panel>
						</Panel>
					</Carousel>
					<CarouselNav id="SectionsCarouselNav" class="OverlayPips" carouselid="SectionsCarousel" />
					                                                                                                                                                                                        

				</Panel>

				<Panel class="TrailerCell" onactivate="DOTAShowSplashAdMovie( patch733_promo )">
					<Panel class="PreviewThumbnailContainer">
						<Panel class="PreviewThumbnail"/>
						<Panel class="PlayIcon">
							<Image src="file://{images}/control_icons/playbutton.psd" />
						</Panel>
					</Panel>
					<Label text="#DOTA_Artifact_Promo_WatchTrailer"/>
				</Panel>

				                                                                                                                                                                                        
			
			
			</Panel>

		</Panel>

	</Panel>

</root>
