<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/dashboard_page_shared.css" />
		<include src="file://{resources}/styles/dashboard_page_coach_rating_gallery.css" />
	</styles>

	<DOTAReferencePage class="DashboardPage SafeDashboardPage">

		<Label class="CoachRatingCategory" text="#DOTACoachRatingProgress" />
		<Panel id="ProgressRatings" class="CoachRatingsList">
			<DOTACoachRatingProgress coach-rating="0" />
			<DOTACoachRatingProgress coach-rating="1" />
			<DOTACoachRatingProgress coach-rating="100" />
			<DOTACoachRatingProgress coach-rating="2500" />
			<DOTACoachRatingProgress coach-rating="5000" />
			<DOTACoachRatingProgress coach-rating="6000" />
			<DOTACoachRatingProgress coach-rating="10000" />
		</Panel>
		
		<Label class="CoachRatingCategory" text="#DOTACoachRatingBadge" />
		<Panel id="BadgeRatings" class="CoachRatingsList">
			<DOTACoachRatingBadge coach-rating="0" />
			<DOTACoachRatingBadge coach-rating="1" />
			<DOTACoachRatingBadge coach-rating="5000" />
			<DOTACoachRatingBadge coach-rating="10000" />
		</Panel>

		<Label class="CoachRatingCategory" text="#DOTACoachRatingIcon" />
		<Panel id="IconRatings" class="CoachRatingsList">
			<DOTACoachRatingIcon coach-rating="0" />
			<DOTACoachRatingIcon coach-rating="1" />
			<DOTACoachRatingIcon coach-rating="5000" />
			<DOTACoachRatingIcon coach-rating="10000" />
		</Panel>

	</DOTAReferencePage>
</root>