<root>

	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/dashboard_page_shared.css" />
		<include src="file://{resources}/styles/dashboard_page_learn_shared.css" />
		<include src="file://{resources}/styles/hero_stat_icons.css" />
		<include src="file://{resources}/styles/dashboard_page_learn_glossary.css" />
		<include src="file://{resources}/styles/dashboard_page_learn_glossary_overview.css" />
	</styles>

	<Panel class="GlossaryPage">

		    

		                                                                                                                                                                                                                                              
		                               	

			                                                                         

			                                                    
			        

			                                       

				                                   
					                                                           
				        

				                                   
					                                                        
				        

				                                   
					                                                           
				        

			        
		            

		                       
		<Panel class="GlossarySection">	

			
			<Panel class="GlossarySectionContents">
			
				
				<Panel class="GlossaryImageContainerLeft" >
					<Panel class="HeroUIContainer">
						<Panel class="HeroUIImageContainer">
							<Panel class="HeroUIImage" />
						</Panel>
					</Panel>
				</Panel> 

				<Panel class="GlossaryTextContainerRight" >
					<Label class="GlossarySectionLabelPreHeader" text="#DOTA_Glossary_TheBasics" />
					<Label class="GlossarySectionLabel" text="#DOTA_Glossary_IntroHeroes" />
					<Panel class="GlossaryIntroBullet">
						<Label text="#DOTA_Glossary_IntroHeroes_Map" />
					</Panel>

					<Panel class="GlossaryIntroBullet">
						<Label text="#DOTA_Glossary_IntroHeroes_Types" />
					</Panel>

					<Panel class="GlossaryIntroBullet">
						<Label text="#DOTA_Glossary_IntroHeroes_RangedMelee" />
					</Panel>

					<Panel class="GlossaryIntroBullet">
						<Label text="#DOTA_Glossary_IntroHeroes_Abilities" />
						<Panel class="InfoIcon GlossaryFootnote" onmouseover="UIShowTextTooltip( #DOTA_Glossary_IntroHeroes_Abilities_Tooltip )" onmouseout="UIHideTextTooltip()"/>
					</Panel>
				</Panel>

			</Panel>
		</Panel>

		
		
		                        
		<Panel class="GlossarySection">	
			
			<Panel class="GlossarySectionContentsDark">
			
				<Panel class="GlossaryTextContainerRight" >
					<Label class="GlossarySectionLabelPreHeader" text="#DOTA_Glossary_TheBasics" />
					<Label class="GlossarySectionLabel" text="#DOTA_Glossary_IntroItems" />
					<Panel class="GlossaryIntroBullet">
						<Label text="#DOTA_Glossary_IntroItems_Agument" />
					</Panel>

					<Panel class="GlossaryIntroBullet">
						<Label text="#DOTA_Glossary_IntroItems_MainShop" />
					</Panel>

					<Panel class="GlossaryIntroBullet">
						<Label text="#DOTA_Glossary_IntroItems_Consumables" />
						<Panel class="InfoIcon GlossaryFootnote" onmouseover="UIShowTextTooltip( #DOTA_Glossary_IntroItems_Consumables_Tooltip )" onmouseout="UIHideTextTooltip()"/>
					</Panel>

					<Panel class="GlossaryIntroBullet">
						<Label text="#DOTA_Glossary_IntroItems_SecretShop" />
					</Panel>

					<Panel class="GlossaryIntroBullet">
						<Label text="#DOTA_Glossary_IntroItems_Combining" />
					</Panel>
				</Panel>
			
				<Panel class="GlossaryImageContainerLeft" >
					<Panel class="ShowcaseScenePanelBlocker">
