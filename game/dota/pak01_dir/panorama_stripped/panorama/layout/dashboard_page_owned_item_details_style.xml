<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
    <include src="file://{resources}/styles/dashboard_page_owned_item_details.css" />
		<include src="file://{resources}/styles/dashboard_page_owned_item_details_style.css" />
 	</styles>
  <Panel class="EditRow">
    <Panel class="StyleRow">
      <Panel class="StyleIcon"/>
      <Label class="StyleName" text="#DOTA_econ_item_details_style_name"/>
    </Panel>
    <Button class="ButtonBevel StyleButton SelectedButton" disabled="true">
      <Label text="#DOTA_econ_item_details_style_selected"/>
    </Button>
    <Button class="ButtonBevel StyleButton AvailableButton" id="StyleAvailable" >
      <Label text="#DOTA_econ_item_details_style_available"/>
    </Button>
		<Button class="ButtonBevel StyleButton UnlockButton" id="StyleUnlock" >
			<Label text="#DOTA_econ_item_details_style_unlock"/>
		</Button>
		<Panel class="InfoIcon AutoStyleInfo" onmouseover="UIShowTitleTextTooltip( #DOTA_econ_item_details_tooltip_auto_style_title, #DOTA_econ_item_details_tooltip_auto_style )" onmouseout="UIHideTitleTextTooltip()" />
		<Panel class="ButtonBevel StyleButton AutoStyleButton" disabled="true" >
			<Label text="#DOTA_econ_item_details_auto_style"/>
    </Panel>
    <Panel class="InfoIcon StyleLockInfo" onmouseover="UIShowTitleTextTooltip( #DOTA_econ_item_details_tooltip_lock_reason_title, #DOTA_econ_item_details_tooltip_lock_reason )" onmouseout="UIHideTitleTextTooltip()" />
    <Button class="ButtonBevel StyleButton UnavailableButton" disabled="true">
      <Label text="#DOTA_econ_item_details_style_locked"/>
    </Button>
  </Panel>
 </root>