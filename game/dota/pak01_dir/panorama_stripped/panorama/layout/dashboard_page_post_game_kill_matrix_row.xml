<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/dashboard_page_shared.css" />
		<include src="file://{resources}/styles/dashboard_page_post_game.css" />
	</styles>

	<DOTAPostGameKillMatrixRow class="StatRowHeight" >
		<Panel class="PlayerRowContainer">
			<Panel class="KillContainer">
				<DOTAHeroImage id="VictimHero0" class="VictimHero" heroimagestyle="icon" />
				<Label id="VictimDeathCount0" class="VictimDeathCount" text="#dota_page_match_details_victim0_count" html="true"/>
			</Panel>
			<Panel class="KillContainer">
				<DOTAHeroImage id="VictimHero1" class="VictimHero" heroimagestyle="icon" />
				<Label id="VictimDeathCount1" class="VictimDeathCount" text="#dota_page_match_details_victim1_count" html="true"/>
			</Panel>
			<Panel class="KillContainer">
				<DOTAHeroImage id="VictimHero2" class="VictimHero" heroimagestyle="icon" />
				<Label id="VictimDeathCount2" class="VictimDeathCount" text="#dota_page_match_details_victim2_count" html="true"/>
			</Panel>
			<Panel class="KillContainer">
				<DOTAHeroImage id="VictimHero3" class="VictimHero" heroimagestyle="icon" />
				<Label id="VictimDeathCount3" class="VictimDeathCount" text="#dota_page_match_details_victim3_count" html="true"/>
			</Panel>
			<Panel class="KillContainer">
				<DOTAHeroImage id="VictimHero4" class="VictimHero" heroimagestyle="icon" />
				<Label id="VictimDeathCount4" class="VictimDeathCount" text="#dota_page_match_details_victim4_count" html="true"/>
			</Panel>
			<Panel id="TotalKills" class="KillContainer">
				<Label class="KillMatrixTotalKills" text="{d:total_kills}" />
			</Panel>		
		</Panel>

	</DOTAPostGameKillMatrixRow>
</root>