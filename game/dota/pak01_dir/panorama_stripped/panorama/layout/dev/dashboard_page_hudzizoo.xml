<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/dashboard_page_shared.css" />
		<include src="file://{resources}/styles/dev/dashboard_page_particle_playground.css" />
	</styles>

	<DOTAReferencePage class="ParticlePlayground DashboardPage">
		<Label class="TitleMedium PageTitle" text="Particle Playground" />
		<Panel class="ParticleGrid">
			<DOTAScenePanel class="ParticleWindow" map="maps/personal/sacha/sacha_smoke_test.vmap" light="hero_light" camera="hero_camera" particleonly="true" renderdeferred="true" />
			<DOTAParticleScenePanel class="ParticleWindow" hittest="false" particleName="particles/dev/library/sacha_test_spin.vpcf" renderdeferred="true" particleonly="false" startActive="true" cameraOrigin="0 200 0" lookAt="0 0 0" fov="30" squarePixels="true" />
			<DOTAParticleScenePanel class="ParticleWindow" hittest="false" particleName="particles/dev/library/sacha_test_spin_2.vpcf" renderdeferred="true" particleonly="false" startActive="true" cameraOrigin="0 200 0" lookAt="0 0 0" fov="30" squarePixels="true" />
		</Panel>
	</DOTAReferencePage>

</root>