<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/dota_ui_ability_build.css" />
	</styles>

	<snippets>
		<snippet name="AbilityRow">
			<Panel class="AbilityRow" >
        <Label class="AbilityName" text="{s:ability_name}" />
        <DOTAAbilityImage id="AbilityImage" />
        <Panel id="TalentImage" onactivate="DOTAAbilityBuildStatBranchSelected()" />
        <Panel id="LevelsContainer">
                                                        
        </Panel> 
			</Panel>
		</snippet>

		<snippet name="LevelSquare">
			<Panel class="LevelSquare" >
				<Label id="LevelLabel" text="{u:level_number}" />
			</Panel>
		</snippet>
	</snippets>
	
	<DOTAAbilityBuild>
		<Panel id="AbilitiesContainer">
			                                             
		</Panel>
	</DOTAAbilityBuild>
</root>

