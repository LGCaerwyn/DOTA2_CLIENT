<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/hudstyles.css" />
		<include src="file://{resources}/styles/dota_ui_shop_item.css" />
	</styles>
	<DOTAShopItem>
		<DOTAItemImage id="ItemImage" hittest="false" scaling="cover" />
		<Panel id="CanPurchaseOverlay" hittest="false" />
		<Panel id="PopularOverlay" hittest="false">
			<Panel id="PopularIcon" hittest="false"/>
		</Panel>
		<Panel id="SuggestedNeutralOverlay" hittest="false">
			<Panel id="SuggestedNeutralIcon" hittest="false"/>
		</Panel>
		<Panel id="PlayerSuggestedOverlay" hittest="false" />
		<Panel id="OwnedTick" hittest="false" />
		<Panel id="SecretShop" hittest="false" />
		<Image id="MarkForBuyIcon" hittest="false" src="file://{images}/hud/reborn/gold_small.psd"/>
		<Image id="BuybackProtectedIcon" hittest="false" src="file://{images}/hud/buyback_protected.png"/>
		<Label id="StockAmount" text="{u:stock_amount}" />
		<Panel id="OutOfStockOverlay" hittest="false" />
		<Panel id="AvailableAtOtherShopOverlay" hittest="false" />
		<Panel id="AvailableAtOtherShopNeedGoldOverlay" hittest="false" />
		<Label id="SuggestionPct" hittest="false" text="{s:suggestion_pct}%"/>
		<Panel id="SequenceArrow" hittest="false"/>
		<Panel id="DotaOfrenda" hittest="false"/>
		<Panel id="DotaSuggest" hittest="false"/>
		<Panel id="ActionOverlay" hittest="false"/>
	</DOTAShopItem>
</root>
