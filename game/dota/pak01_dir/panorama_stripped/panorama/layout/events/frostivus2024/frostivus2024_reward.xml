<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/events/frostivus2024/frostivus2024_reward.css" />
	</styles>

	<snippets>
		
		<snippet name="EconItemIcon">
			<Panel class="EconItemIcon">
				<DOTAEconItem id="EconItem" />
				<Label class="Quantity" text="x{d:quantity}" />
			</Panel>
		</snippet>

		<snippet name="EconItemModel">
			<Panel class="EconItemModel">
				<Button id="Inspect" onactivate="DOTAFrostivus2024InspectClicked()" />
				<DOTAUIEconSetPreview id="EconItemModelPreview" drawbackground="false" displaymode="loadout_small" suppress-intro-effects="true" hittest="false" hittestchildren="false" />
			</Panel>
		</snippet>

		<snippet name="ChatWheel">
			<Panel class="ChatWheel">
				<Label text="Chat Wheel" />
			</Panel>
		</snippet>

		<snippet name="OverworldTokens">
			<Panel class="OverworldTokens">
				<Panel id="OverworldTokenList">
					                                
				</Panel>
				<Label class="OverworldName" text="{s:overworld_name}" />
			</Panel>
		</snippet>

		<snippet name="ActionList">
			<Panel class="ActionList">
				<Panel class="Header">
					<Panel class="RandomIcon" />
					<Label class="ActionListType" text="#DOTA_BattlePass_RandomReward" />
					<Panel class="InfoIcon" onmouseover="UIShowTextTooltip( '#DOTA_BattlePass_RandomReward_Info' )" onmouseout="UIHideTextTooltip()" />
				</Panel>
				<Panel id="ActionListContents">
					                                  
				</Panel>
			</Panel>
		</snippet>

		<snippet name="ActionListReward">
			<Panel class="ActionListReward">
				
			</Panel>
		</snippet>
		
		<snippet name="ActionListReward_ChatWheelMessages">
			<Panel class="ChatWheelMessages">
				<Panel class="AlreadyClaimedIcon" />
				<Panel id="Messages">
					                                      
				</Panel>
			</Panel>
		</snippet>

		<snippet name="ActionListReward_Emoticon">
			<Panel class="Emoticon">
				<Panel id="Emoticons">
					                              
				</Panel>
				<Panel class="AlreadyClaimedIcon" />
			</Panel>
		</snippet>

	</snippets>

	<DOTAFrostivus2024Reward>
		
	</DOTAFrostivus2024Reward>
	
</root>
