<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/frontpage_shared.css" />
		<include src="file://{resources}/styles/events/newbloom2025_frontpage.css" />
	</styles>

	<DotaNewBloom2025FrontPageCell class="DotaNewBloom2025FrontPageCell" hittest="true" onactivate="DOTAShowTreasurePage( 32658 )">
		<Panel class="CellBackground" />
		<Panel class="HeroModels">
			<DOTAUIEconSetPreview id="NB_Medusa" itemdef="32659" cameraOrigin="0 0 0" rotation="0" drawbackground="false" require-composition-layer="true" suppress-intro-effects="false" />
			<DOTAUIEconSetPreview id="NB_Necrophos" itemdef="32668" cameraOrigin="0 0 0" rotation="-45" drawbackground="false" require-composition-layer="true" suppress-intro-effects="false" />
		</Panel>
		<Panel class="CellDarkness" />
		<Panel class="CellBorder" />
		<Panel class="CellContents" hittest="false">
			<Label class="CellTitle TitleSmall" text="#DashboardBackground_NewBloom2025_Treasure_Name"/>
			<Label class="CellDescription BodySmall" text="#DashboardBackground_NewBloom2025_Treasure_Desc"/>
			<Button id="ViewTreasureButton" class="ButtonPrimary Gold">
				<Label text="#DOTA_Treasure_ViewTreasure" />
			</Button>
		</Panel>
	</DotaNewBloom2025FrontPageCell>
</root>