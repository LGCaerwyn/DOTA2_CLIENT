<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/frontpage_shared.css" />
		<include src="file://{resources}/styles/frontpage_supporters_club.css" />
	</styles>

	<CustomLayoutPanel onactivate="DOTAShowWatchDPCFanContentPage()">
		<Image id="CellImage" class="FrontpageCellImage" src="file://{images}/front_page/dpc_supclub_release.psd" scaling="stretch-to-cover-preserve-aspect" />
		<Label class="FrontpageCellHeader" text="#DOTA_Tournament_DPC" />
		<Panel class="FrontpageCellTint" />
		<Panel class="FrontpageCellFooter">
			<Label class="FrontpageCellText" text="#DOTA_DPCTeams_SupporterClubFPDesc" html="true" />
			<Label class="FrontpageCellTitle" text="#DOTA_DPCTeams_IntroSupporterClub" html="true" />
		</Panel>
		<Label id="SupClubName" text="#DOTA_TeamFanContent_SupportersClub" />
		<Panel class="LeftRightFlow NewBadge">
			<Label id="SupClubNewTeams" text="#DOTA_TeamFanContent_NewTeamsAvail" />
			<Panel id="SupClubNewTeamsEdge" />
		</Panel>
	</CustomLayoutPanel>
</root>
