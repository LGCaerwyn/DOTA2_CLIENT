<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/guild_contract.css" />
	</styles>

	<DOTAGuildContract>
		<Panel id="ContractImageContainer">
			<DOTAHeroImage id="HeroIcon" heroimagestyle="portrait" scaling="stretch-to-cover-preserve-aspect" />
			<Image id="GenericIcon" heroimagestyle="icon" scaling="stretch-to-cover-preserve-aspect" />
		</Panel>
		<Panel id="StarsContainer">
			<Panel id="StarsCenter" class="GuildImageColorAdjusted">
				<Panel id="Star1" class="ContractStar" />
				<Panel id="Star2" class="ContractStar" />
				<Panel id="Star3" class="ContractStar" />
			</Panel>
		</Panel>
		<Panel id="ContractTextCenter">
			<Label id="ContractText" html="true"/>
		</Panel>
		<Label id="EmptyText" text="?" />
	</DOTAGuildContract>

</root>
