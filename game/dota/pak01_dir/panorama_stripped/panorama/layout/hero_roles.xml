<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/hero_roles.css" />
	</styles>

	<snippets>
		<snippet name="RoleRow">
			<Panel class="RoleRow">
				<Label class="BodyMedium LighterGreyText Opacity_50" text="{s:role_name}" />
