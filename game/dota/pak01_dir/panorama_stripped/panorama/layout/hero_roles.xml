				<Panel class="RowPip RowPip2" />
				<Panel class="RowPip RowPip3" />
			</Panel>
		</snippet>
	</snippets>

	<DOTAHeroRoles>
		<Panel class="RoleColumn" />
		<Panel class="RoleColumn" />
	</DOTAHeroRoles>

</root>
