<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/hudstyles.css" />
		<include src="file://{resources}/styles/hud/dota_hero_labels.css" />
	</styles>
	<snippets>
		<snippet name="HeroLabel">
			<Panel class="HeroLabel">
				<Label style="margin-left: 0px; margin-top: 0px;" text="{s:hero_label}"/>
				<Label style="margin-left: 3px; margin-top: 0px;" text="{s:hero_label}"/>
				<Label style="margin-left: 6px; margin-top: 0px;" text="{s:hero_label}"/>
				<Label style="margin-left: 0px; margin-top: 3px;" text="{s:hero_label}"/>
				<Label style="margin-left: 0px; margin-top: 6px;" text="{s:hero_label}"/>
				<Label style="margin-left: 3px; margin-top: 6px;" text="{s:hero_label}"/>
				<Label style="margin-left: 6px; margin-top: 3px;" text="{s:hero_label}"/>
				<Label style="margin-left: 6px; margin-top: 6px;" text="{s:hero_label}"/>
																			
				<Label class="MainLabel" style="margin-left: 3px; margin-top: 3px; color:#FFFFFFFF;" text="{s:hero_label}"/>
			</Panel>
		</snippet>
		<snippet name="TeamLabel">
			<Panel class="TeamLabel">
				<Label style="margin-left: 0px; margin-top: 0px;" text="{s:team_label}"/>
				<Label style="margin-left: 3px; margin-top: 0px;" text="{s:team_label}"/>
				<Label style="margin-left: 6px; margin-top: 0px;" text="{s:team_label}"/>
				<Label style="margin-left: 0px; margin-top: 3px;" text="{s:team_label}"/>
				<Label style="margin-left: 0px; margin-top: 6px;" text="{s:team_label}"/>
				<Label style="margin-left: 3px; margin-top: 6px;" text="{s:team_label}"/>
				<Label style="margin-left: 6px; margin-top: 3px;" text="{s:team_label}"/>
				<Label style="margin-left: 6px; margin-top: 6px;" text="{s:team_label}"/>

				<Label class="MainLabel" style="margin-left: 3px; margin-top: 3px; color:#FFFFFFFF;" text="{s:team_label}"/>
			</Panel>
		</snippet>
		<snippet name="SponsorLabel">
			<Panel class="SponsorLabel">
				<Label style="margin-left: 0px; margin-top: 0px;" text="{s:sponsor_label}"/>
				<Label style="margin-left: 3px; margin-top: 0px;" text="{s:sponsor_label}"/>
				<Label style="margin-left: 6px; margin-top: 0px;" text="{s:sponsor_label}"/>
				<Label style="margin-left: 0px; margin-top: 3px;" text="{s:sponsor_label}"/>
				<Label style="margin-left: 0px; margin-top: 6px;" text="{s:sponsor_label}"/>
				<Label style="margin-left: 3px; margin-top: 6px;" text="{s:sponsor_label}"/>
				<Label style="margin-left: 6px; margin-top: 3px;" text="{s:sponsor_label}"/>
				<Label style="margin-left: 6px; margin-top: 0px;" text="{s:sponsor_label}"/>
				
				<Label class="MainLabel" style="margin-left: 3px; margin-top: 3px; color:#FFFFFFFF;" text="{s:sponsor_label}"/>
			</Panel>
		</snippet>
		<snippet name="StatusLabel">
			<Panel class="StatusLabel">
				<Label style="margin-left: 0px; margin-top: 0px;" text="{s:status_name}"/>
				<Label style="margin-left: 3px; margin-top: 0px;" text="{s:status_name}"/>
				<Label style="margin-left: 6px; margin-top: 0px;" text="{s:status_name}"/>
				<Label style="margin-left: 0px; margin-top: 3px;" text="{s:status_name}"/>
				<Label style="margin-left: 0px; margin-top: 6px;" text="{s:status_name}"/>
				<Label style="margin-left: 3px; margin-top: 6px;" text="{s:status_name}"/>
				<Label style="margin-left: 6px; margin-top: 3px;" text="{s:status_name}"/>
				<Label style="margin-left: 6px; margin-top: 6px;" text="{s:status_name}"/>				
				<Label class="MainLabel" style="margin-left: 3px; margin-top: 3px; color:#FFFFFFFF;" text="{s:status_name}"/>
			</Panel>
		</snippet>
	</snippets>
	<Panel hittest="false" hittestchildren="false" style="flow-children: right-wrap;">
	</Panel>
</root>
