<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/hudstyles.css" />
		<include src="file://{resources}/styles/hud/dota_hud_bruiser_spell_card.css" />
	</styles>
	<DOTABruiserSpellCardRow hittest="false">
		<DOTAAbilityImage id="AbilityIcon" hittest="true" showtooltip="true" />
		<Panel id="KeyContainer0" class="KeyContainer" hittest="false" >
			<Label id="InvokeKey0" class="InvokeKey" hittest="false" />
			<Label id="InvokeModifier0" class="InvokeModifier" hittest="false"  />
		</Panel>
		<Panel id="KeyContainer1" class="KeyContainer" hittest="false" >
			<Label id="InvokeKey1" class="InvokeKey" hittest="false" html="true" />
			<Label id="InvokeModifier1" class="InvokeModifier" hittest="false"  />
		</Panel>
		<Panel id="KeyContainer2" class="KeyContainer" hittest="false" >
			<Label id="InvokeKey2" class="InvokeKey" hittest="false" html="true" />
			<Label id="InvokeModifier2" class="InvokeModifier" hittest="false"  />
		</Panel>
		<Panel id="KeyContainer3" class="KeyContainer" hittest="false" >
			<Label id="InvokeKey3" class="InvokeKey" hittest="false" html="true" />
			<Label id="InvokeModifier3" class="InvokeModifier" hittest="false"  />
		</Panel>
		<Panel id="KeyContainer4" class="KeyContainer" hittest="false" >
			<Label id="InvokeKey4" class="InvokeKey" hittest="false" html="true" />
			<Label id="InvokeModifier4" class="InvokeModifier" hittest="false"  />
		</Panel>
		<Label id="AbilityName" text="{s:ability_name}" hittest="false" />
	</DOTABruiserSpellCardRow>
</root>
