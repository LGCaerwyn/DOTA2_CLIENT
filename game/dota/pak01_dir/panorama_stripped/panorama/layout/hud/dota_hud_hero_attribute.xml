<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/hud/dota_hud_hero_attribute.css" />
	</styles>

	<DOTAHudHeroAttribute>
		<Panel id="Icon" />
		<Panel id="Labels">
			<Panel id="Title">
				<Label text="#DOTA_Hud_AttributeTooltip_Title" />
				<Label class="PrimaryOnly" text="#DOTA_Hud_AttributeTooltip_Primary" />
			</Panel>
			<Label text="#DOTA_Hud_AttributeTooltip_Damage" class="PrimaryOnly" />
			<Panel id="StrengthLabels">
				<Label text="#DOTA_Hud_StrTooltip_HP" />
				<Label text="#DOTA_Hud_StrTooltip_HPRegen" />
			</Panel>
			<Panel id="AgilityLabels">
				<Label text="#DOTA_Hud_AgiTooltip_Armor" />
				<Label text="#DOTA_Hud_AgiTooltip_AttackSpeed" />
			</Panel>
			<Panel id="IntelligenceLabels">
				<Label text="#DOTA_Hud_IntTooltip_Mana" />
				<Label text="#DOTA_Hud_IntTooltip_ManaRegen" />
				<Label text="#DOTA_Hud_IntTooltip_SpellDamage" />
			</Panel>
			<Label text="#DOTA_Hud_Attribute_PointsPerLevel" />
		</Panel>
	</DOTAHudHeroAttribute>
</root>
