<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/hudstyles.css" />
		<include src="file://{resources}/styles/hud/hud_reborn.css" />
		<include src="file://{resources}/styles/custom_ui.css" />
		<include src="file://{resources}/styles/collection_shared.css" />
		<include src="file://{resources}/styles/hud/dota_hud_innate_display.css" />
	</styles>

	<DOTAInnateDisplay class="RootInnateDisplay" >
		<Panel id="ContentsContainer">
			<Panel class="FacetHolder" onactivate="DOTAActivateInnateDisplayFacet()" oncontextmenu="DOTAFacetContextMenu()" onmouseover="DOTAShowFacetDisplayTooltip()" onmouseout="DOTAHideFacetDisplayTooltip()">
				<DOTAFacetIcon id="FacetIcon" hittest="false" class="Darken" hittestchildren="false" />
			</Panel>
			<DOTAInnateIcon id="InnateIcon" class="Small" show-tooltips="false" onactivate="DOTAActivateInnateDisplayInnate()" oncontextmenu="DOTAInnateContextMenu()" onmouseover="DOTAShowInnateDisplayTooltip()" onmouseout="DOTAHideInnateDisplayTooltip()" />
		</Panel>
	</DOTAInnateDisplay>
</root>