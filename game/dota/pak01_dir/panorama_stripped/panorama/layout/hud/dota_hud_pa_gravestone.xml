<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/hudstyles.css" />
		<include src="file://{resources}/styles/hud/dota_hud_pa_gravestone.css" />
	</styles>
	<DOTAPAGravestone>
		<Label id="title" text="{s:epitaph}" />
		<DOTAHeroImage id="hero" />
		<Label id="player" text="{s:victim_player}" />
		<Panel id="damageRow" >
			<Label id="coup" text="#DOTA_Tooltip_ability_phantom_assassin_coup_de_grace" />
			<Label id="colon" text=":" />
			<Label id="critDamage" text="{u:crit_damage}" />
			<Label id="damage" text="#DOTA_AttribDamage" />
		</Panel>
		<Label id="contract" text="#DOTA_Chat_AssassinFulfill03" />
	</DOTAPAGravestone>
</root>


