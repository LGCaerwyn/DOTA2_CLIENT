<root>
    <styles>
        <include src="file://{resources}/styles/dotastyles.css" />
        <include src="file://{resources}/styles/hudstyles.css" />
        <include src="file://{resources}/styles/hud/dota_hud_top_bar.css" />
    </styles>

    <DOTAPlayerModifier>
				<Panel class="PlayerModifierItemPanel" hittest="false">
					<DOTAParticleScenePanel id="PlayerModifiedParticleCheese" hittest="false" particleName="particles/ui/topbar/topbar_ambient_cheese.vpcf" particleonly="true" startActive="true" cameraOrigin="130 0 0" lookAt="0 0 80" fov="60" squarePixels="true" />
					<DOTAParticleScenePanel id="PlayerModifiedParticleAegis" hittest="false" particleName="particles/ui/topabar/aegis_bg.vpcf" particleonly="true" startActive="true" cameraOrigin="0 0 150" lookAt="0 90 0" fov="60" squarePixels="true" />
					<DOTAParticleScenePanel id="PlayerModifiedParticleSmoke" hittest="false" particleName="particles/ui/topbar/topbar_ambient_smoke_of_deceit.vpcf" particleonly="true" startActive="true" cameraOrigin="0 150 0" lookAt="0 0 40" fov="60" squarePixels="true" />
					<DOTAParticleScenePanel id="PlayerModifiedParticleSmokeBottle" hittest="false" particleName="particles/ui/topbar/topbar_ambient_smoke_of_deceit_bottle.vpcf" particleonly="true" startActive="true" cameraOrigin="15 0 0" lookAt="0 0 0" fov="60" squarePixels="true" />
        	<Panel id="PlayerModifierItemImage" />
					<DOTAParticleScenePanel id="PlayerModifiedParticleAegisSparks" hittest="false" particleName="particles/generic_gameplay/dropped_aegis_e.vpcf" particleonly="true" startActive="true" cameraOrigin="0 90 0" lookAt="0 0 40" fov="60" squarePixels="true" />
        </Panel>
    </DOTAPlayerModifier>
</root>
