<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/hud/dota_hud_plus_status.css" />
	</styles>

	<DOTAPlusStatus class="QuestStatusHidden" hittest="false">
		<Panel id="QuestStatusContainer" hittest="false" class="TopBottomFlow">
			<Panel id="QuestHeader" class="LeftRightFlow">
				<Button id="QuestExpandButton1" class="QuestExpandButton">
					<Panel class="CheckIcon QuestStatusIcon"/>
					<Panel class="XIcon QuestStatusIcon"/>
				</Button>
				<Label id="PrimaryProgress" class="ProgressNumbers" text="{d:primary_progress} / {d:primary_goal}" hittest="false"/>
				<Panel id="ChallengeStars" class="LeftRightFlow">
					<Panel class="Star Rank1"/>
					<Panel class="Star Rank2"/>
					<Panel class="Star Rank3"/>
				</Panel>
			</Panel>
			<Label id="ChallengeDesc" class="ProgressNumbers"/>
		</Panel>
		
	</DOTAPlusStatus>
</root>


