<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/hudstyles.css" />
		<include src="file://{resources}/styles/hud/dota_hud_ringmaster_souvenir_card.css" />
	</styles>
	<DOTARingmasterSouvenirCard hittest="false" class="Hidden">
		<Panel class="SouvenirCardContainer">
			<Panel class="SouvenirBorder"/>
			<Panel id="SouvenirHeader">
				<Label id="SouvenirHeaderLabel" text="#DOTA_HUD_RingmasterSouvenir_Header"/>
			</Panel>
			<Panel id="AbilityContainer" />
			<Button id="CloseSouvenirCard" class="ButtonPrimary DarkRed Small"  onactivate="DOTAToggleRingmasterSouvenirCard( 0 )">
				<Label text="#DOTA_Close"/>
			</Button>
		</Panel>
		<Panel class="HitBlocker"/>
	</DOTARingmasterSouvenirCard>
</root>
