<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/hudstyles.css" />
		<include src="file://{resources}/styles/hud/dota_hud_spectre_arcana.css" />
	</styles>
	<DOTAHUDSpectreArcana hittest="false">

		<Panel id="ProgressPanel" hittest="false" hittestchildren="false">
			<Panel class="CircleBG" />
			<Panel class="ProgressBG" />
			<DOTAParticleScenePanel id="ParticleBG" class="ParticleBG" hittest="false" particleName="particles/econ/items/spectre/spectre_arcana/spectre_arcana_hud_ambient.vpcf" particleonly="true" cameraOrigin="0 0 300" lookAt="0 0 0" fov="40"/>

			<DOTAParticleScenePanel id="ParticleBurst" class="ParticleBG" hittest="false" particleName="particles/econ/items/spectre/spectre_arcana/spectre_arcana_hud_killstreak_burst.vpcf" startActive="false"  particleonly="true" cameraOrigin="0 0 300" lookAt="0 0 0" fov="40"/>
			
			<Panel class="ProgressPanelContent">
				
				<ProgressBar id="InGameArcanaProgressBar" class="ProgressBar"/>

				<Panel class="ProgressMidLayer"/>

				<Label class="ProgressKillStreakLabel" html="true" text="#DOTA_Arcana_Spectre_MiniGame_Slideout_Killstreak_Label"/>
				
				<Label class="ProgressKillStreakProgressLabel" html="true" text="#DOTA_Arcana_Spectre_MiniGame_Slideout_Killstreak_ProgressLabel"/>
				<Label class="ProgressKillStreakCompleteLabel" html="true" text="#DOTA_Arcana_Spectre_MiniGame_Slideout_Killstreak_CompleteLabel"/>

				<Panel class="ProgressFGLayer">
					<Panel class="ProgressBlade Blade1"/>
					<Panel class="ProgressBlade Blade2"/>
					<Panel class="ProgressBlade Blade3"/>
					<Panel class="ProgressBlade Blade4"/>
					<Panel class="ProgressBlade Blade5"/>
				</Panel>
			</Panel>
		</Panel>
	</DOTAHUDSpectreArcana>
</root>
