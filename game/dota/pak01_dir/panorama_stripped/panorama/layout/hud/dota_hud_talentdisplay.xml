<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/hudstyles.css" />
		<include src="file://{resources}/styles/hud/hud_reborn.css" />
		<include src="file://{resources}/styles/custom_ui.css" />
		<include src="file://{resources}/styles/hud/dota_hud_stat_branch.css" />		
	</styles>

	<DOTAHudTalentDisplay onactivate="DOTAHUDTalentTreeClicked()" onmouseover="DOTAHUDMouseOverTalentTree()" onmouseout="DOTAHUDMouseOutTalentTree()" >
		<Panel id="StatBranchBG" onactivate="DOTAHUDStatBranchClicked()" />
		<Panel id="StatBranchBGBranchWell"/>
		<Panel id="StatBranchGraphics" require-composition-layer="true" always-cache-composition-layer="true">
			<Panel id="StatBranchChannel">
				<Panel id="StatPipContainer" class="">
					<Panel id="StatRow25" class="StatBranchRow">
						<Panel class="StatBranchPip LeftBranchPip PipBlur"/>
						<Panel class="StatBranchPip RightBranchPip PipBlur"/>
						<Panel class="StatBranchPip LeftBranchPip"/>
						<Panel class="StatBranchPip RightBranchPip"/>
					</Panel>
					<Panel id="StatRow20" class="StatBranchRow">
						<Panel class="StatBranchPip LeftBranchPip PipBlur"/>
						<Panel class="StatBranchPip RightBranchPip PipBlur"/>
						<Panel class="StatBranchPip LeftBranchPip"/>
						<Panel class="StatBranchPip RightBranchPip"/>
					</Panel>
					<Panel id="StatRow15" class="StatBranchRow">
						<Panel class="StatBranchPip LeftBranchPip PipBlur"/>
						<Panel class="StatBranchPip RightBranchPip PipBlur"/>
						<Panel class="StatBranchPip LeftBranchPip"/>
						<Panel class="StatBranchPip RightBranchPip"/>
					</Panel>
					<Panel id="StatRow10" class="StatBranchRow">
						<Panel class="StatBranchPip LeftBranchPip PipBlur"/>
						<Panel class="StatBranchPip RightBranchPip PipBlur"/>
						<Panel class="StatBranchPip LeftBranchPip"/>
						<Panel class="StatBranchPip RightBranchPip"/>
					</Panel>
				</Panel>							
				<Panel class="LevelProgress">
					<ProgressBar id="StatLevelProgressBar" min="0" max="26" value="20" />
				</Panel>
				<Panel class="LevelProgressBlur">
					<ProgressBar id="StatLevelProgressBarBlur" min="0" max="26" value="20" />
				</Panel>
			</Panel>
		</Panel>
		<Panel id="AttributePipsContainer">
			<Panel id="AttributePips" />
		</Panel>
	</DOTAHudTalentDisplay>
</root>