<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/hud/dota_hud_common_items.css" />
	</styles>
	<DOTAHUDTeamItems>
		<Panel id="CommonItemTitleContainer" onmouseout="DotaHideTeammateItemExplainer();">
			<Label id="TeamLabel" class="CommonItemsTitle" text="#dota_item_build_team_items" onmouseover="DotaShowTeammateItemExplainer( 1 );"/>
			<Label id="PinnedLabel" class="CommonItemsTitle" text="#dota_item_build_common_items" onmouseover="DotaShowTeammateItemExplainer( 0 );"/>
		</Panel>
		<Panel id="Contents">
			<Panel id="EmptyLabelContainer"  onmouseover="DotaShowTeammateItemExplainer( 2 );" onmouseout="DotaHideTeammateItemExplainer();">
				<Label id="EmptyLabel" text="#dota_item_build_team_items_empty" html="true"/>
			</Panel>
			<Panel id="ItemColumn">
				<Panel id="PinnedItemList">
						                               
				</Panel>
				<Panel id="TeamItemList">
					                               
				</Panel>
				<Panel id="VerticalDivider" hittest="false" hittestchildren="false"/>
			</Panel>
		</Panel>
	</DOTAHUDTeamItems>
</root>

