<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/hud/dota_hud_unit_damage_armor.css" />
	</styles>

	<DOTAHudUnitDamageArmor>
		<Panel id="Damage">
			<Panel id="DamageIcon" />
			<Panel id="DamageLabels">
				<Label text="#DOTA_Hud_AttackSpeed" html="true" />
				<Label text="#DOTA_Hud_AttackDamage" html="true" />
				<Label text="#DOTA_Hud_AttackRange" html="true" />
				<Label id="SpellDamageAmp" text="#DOTA_Hud_SpellDamageAmp" html="true" />
			</Panel>
		</Panel>
		<Panel id="Armor">
			<Panel id="ArmorIcon" />
			<Panel id="ArmorLabels">
				<Label text="#DOTA_Hud_PhysicalArmor" html="true" />
				<Label text="#DOTA_Hud_DamageResistance" html="true" />
				<Label text="#DOTA_Hud_MagicResistance" html="true" />
			</Panel>
		</Panel>
	</DOTAHudUnitDamageArmor>
</root>
