<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/hudstyles.css" />
		<include src="file://{resources}/styles/hud/dota_hud_xp.css" />
	</styles>
	<DOTAXP hittest="false">
		<Panel id="LevelBackground" onactivate="DOTAHUDXPBarClicked();" oncontextmenu="DOTAHUDXPBarContextMenu();" />
		<Label id="LevelLabel" class="MonoNumbersFont" text="{d:level}" hittest="false" />
		<CircularProgressBar id="CircularXPProgress" onactivate="DOTAHUDXPBarClicked();" oncontextmenu="DOTAHUDXPBarContextMenu();" />
		<CircularProgressBar id="CircularXPProgressBlur" hittest="false"/>
		<Label id="XPLabel" text="#DOTA_Hud_XP" hittest="false" />
		<Label id="LifetimeLabel" class="MonoNumbersFont" text="#DOTA_Hud_Lifetime" hittest="false" />
		<ProgressBar id="LifetimeProgress" class="MonoNumbersFont" hittest="false" />
	</DOTAXP>
</root>


