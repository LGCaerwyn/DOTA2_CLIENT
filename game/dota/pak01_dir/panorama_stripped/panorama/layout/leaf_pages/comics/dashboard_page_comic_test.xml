<root>
	<styles>
		                                                                    
		<include src="file://{resources}/styles/comic_book_styles.css" />
		                                     
		<include src="file://{resources}/styles/leaf_pages/comics/dashboard_page_comic_crystal_maiden.css" />
	</styles>

	                                                                                     
	<DOTAReferencePage class="DashboardPage" oncancel="DOTAShowHomePage()">

		<ComicViewerBook class="">
			<ComicViewerPage class="rows">
				<Panel class="row">
					<ComicViewerScenePanelCell class="layout-1-row-1-cell-1" map="backgrounds/hero_showcase_muerta" camera="hero_camera" light="hero_light" renderdeferred="true"/>
				</Panel>
				<Panel class="row">
					<ComicViewerMovieCell class="layout-1-row-2-cell-1" movie="file://{resources}/videos/intro_movie.webm" autoadvance="true"/>
					<ComicViewerImageCell class="layout-1-row-2-cell-2" image="file://{images}/battlepass/ti7/campaign/comic/01.psd"/>
					<ComicViewerImageCell class="layout-1-row-2-cell-3" image="file://{images}/battlepass/ti7/campaign/comic/01.psd"/>
				</Panel>
			</ComicViewerPage>
			<ComicViewerPage class="rows">
				<Panel class="row">
					<ComicViewerScenePanelCell class="layout-1-row-1-cell-1" map="backgrounds/hero_showcase_muerta" camera="hero_camera" light="hero_light" renderdeferred="true"/>
				</Panel>
				<Panel class="row">
					<ComicViewerMovieCell class="layout-1-row-2-cell-1" movie="file://{resources}/videos/intro_movie.webm" autoadvance="true"/>
					<ComicViewerImageCell class="layout-1-row-2-cell-2" image="file://{images}/battlepass/ti7/campaign/comic/01.psd"/>
					<ComicViewerImageCell class="layout-1-row-2-cell-3" image="file://{images}/battlepass/ti7/campaign/comic/01.psd"/>
				</Panel>
			</ComicViewerPage>
		</ComicViewerBook>
		
	</DOTAReferencePage>
</root>
