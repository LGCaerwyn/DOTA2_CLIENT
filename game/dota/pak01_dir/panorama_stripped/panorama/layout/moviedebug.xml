<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/movie.css" />
	</styles>
	
	<MovieDebug oncanel="DeletePanel( this )">
		<Panel class="DebugBackground">
			<Panel class="DebugColumn">
				<Label text="Dimensions:" unlocalized="true" />
				<Label text="Resolution:" unlocalized="true" />
				<Label text="Video" unlocalized="true" />
				<Label text="- Download:" unlocalized="true" />
				<Label text="- Bandwidth:" unlocalized="true" />
			</Panel>
			<Panel class="DebugColumn DebugSecondColumn">
				<Label id="Dimensions" />
				<Label id="Resolution" />
				<Label text=" " />
				<Label id="VideoSegment" />
				<Label id="VideoBandwidth" />
			</Panel>
		</Panel>
	</MovieDebug>
</root>
