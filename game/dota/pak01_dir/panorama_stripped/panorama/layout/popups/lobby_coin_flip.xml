<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/popups/popups_shared.css" />
		<include src="file://{resources}/styles/popups/lobby_coin_flip.css" />
	</styles>
	
	<Popup class="PopupPanel CoinFlipContainer Hidden" popupbackground="dim" oncancel="UIPopupButtonClicked()">
		<Panel class="Fence">
			<Panel id="CoinContainer">
				<Panel class="Tails CoinSide">
				<Label class="Test" text="-1-"/>
					<Panel class="TeamLogoContainer">
						<Panel class="TeamLogo LogoShadow"/>
						<Panel class="TeamLogo LogoHighlight"/>
						<Panel class="TeamLogo"/>
					</Panel>
				</Panel>
				<Panel class="Heads CoinSide">
				<Label class="Test" text="-2-"/>
					<Panel class="TeamLogoContainer">
						<Panel class="TeamLogo LogoShadow"/>
						<Panel class="TeamLogo LogoHighlight"/>
						<Panel class="TeamLogo"/>
					</Panel>
				</Panel>
			</Panel>
		</Panel>
		<Panel class="FenceEdge">
			<Panel id="CoinContainerEdge"/>
		</Panel>
		<Panel class="CloseButton" onactivate="UIPopupButtonClicked()"/>
	</Popup>
</root>