<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/popups/popups_shared.css" />
		<include src="file://{resources}/styles/popups/popup_compendium_level.css" />
	</styles>

	<PopupCompendiumLevel class="PopupPanel Hidden" popupbackground="blur" oncancel="UIPopupButtonClicked()" >
		<Panel class="BackgroundImage"/>
		<Panel class="MainBox" >
			<Label id="HeaderLabel" text="#BattlePass_LevelUp_Header" />
			<Label id="BodyLabel" text="#BattlePass_LevelUp_Body" />
			<Panel id="BlackBanner">
				<Panel id="LeftSide">
					<Panel id="CompendiumLevelAndNumber">
						<Label id="CompendiumLevelLabel" text="#TI5_Level" />
						<Label id="CompendiumLevelNumber" class="MonoNumbersFont" text="#DOTA_Compendium_Level_Number" />
					</Panel>
				</Panel>
				<Panel id="VerticalLine" />
				<Panel id="RightSide">
					<ProgressBar id="CompendiumProgress" />
					<Label id="Points" text="#DOTA_Compendium_Level_Points" />
					<Label id="PointsRemainingLabel" text="#TI5_LevelUp_PointsRemaining" />
				</Panel>
			</Panel>
			<DOTAInternational2024Button class="ContinueButton LightGreen" text="#DOTA_UI_Confirmation_Continue" onactivate="UIPopupButtonClicked()" />
		</Panel>
		<Panel class="TopDash"/>
		<Panel class="TopDash Bottom"/>
		<Panel class="PopupBorder" hittest="false"/>
	</PopupCompendiumLevel>

</root>
