<root>
	<styles>
    <include src="file://{resources}/styles/dotastyles.css" />
    <include src="file://{resources}/styles/popups/popups_shared.css" />
    <include src="file://{resources}/styles/popups/popup_confirm_emoticon_unlock.css" />
  </styles>

  <PopupConfirmEmoticonUnlock class="PopupPanel Hidden" popupbackground="dim">
    <Label id="TitleLabel" class="PopupTitle" text ="#DOTA_Emoticon_Unlock_Confirm_Title" />
    <Label id="MessageLabel" text ="#DOTA_Emoticon_Unlock_Confirm" />
    <Panel id="EmoticonsList">
                                    
    </Panel>
    <Panel class="PopupButtonRow">
      <Button id="OkButton" class="PopupButton" onactivate="UIPopupButtonClicked( DOTAConfirmEmoticonUnlock( true ) )">
        <Label text="#UI_OK" />
      </Button>
      <Button id="CloseButton" class="PopupButton" onactivate="UIPopupButtonClicked( DOTAConfirmEmoticonUnlock( false ) )">
        <Label text="#Cancel" />
      </Button>
    </Panel>
  </PopupConfirmEmoticonUnlock>
</root>
