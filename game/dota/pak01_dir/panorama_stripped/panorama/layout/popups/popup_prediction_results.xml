<root>
	<styles>
        <include src="file://{resources}/styles/popups/popup_prediction_results.css" />
        <include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/popups/popups_shared.css" />
	</styles>

  <DOTAPopupPredictionResults class="PopupPanel Hidden" popupbackground="dim" oncancel="UIPopupButtonClicked()">
    <Panel id="TopHeader">
      <Label html="true" id="PredictionTitle" text="{s:prediction_name}"/>
      <Panel class="FillWidth"/>
      <Button class="CloseButton" id="PrevArrowButton" onactivate="UIPopupButtonClicked()"/>
    </Panel>
		<Panel id="MainSectionContainer" class="LeftRightFlow">
      <Panel id="Header">
        <Panel id="Rank">
          <Label text="#DOTA_Compendium_Rank"/>
        </Panel>
        <Panel id="Prediction">
          <Label text="#DOTA_Compendium_Prediction"/>
        </Panel>
        <Panel id="Amount">
          <Label text="#DOTA_Compendium_Amount"/>
        </Panel>
        <Panel id="Community">
          <Label text="#DOTA_Compendium_Votes"/>
        </Panel>
      </Panel>
      
			<Panel id="Body">
				
			</Panel>
			
		</Panel>
	</DOTAPopupPredictionResults>
</root>
