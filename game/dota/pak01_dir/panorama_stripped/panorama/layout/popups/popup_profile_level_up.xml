<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/popups/popups_shared.css" />
		<include src="file://{resources}/styles/popups/popup_profile_level_up.css" />
	</styles>

	<PopupProfileLevelUp class="PopupPanel Hidden" popupbackground="blur" hittest="false" oncancel="UIPopupButtonClicked()" >
		<Panel class="BlackBackground"/>
    	<DOTAScenePanel id="TrophyLvlUp" map="scenes/badge_level_up" camera="shot_camera" particleonly="true"/>
		<Panel class="MainContainer">
			<Label id="ProfileBadgeUpgradedLabel" text="#DOTA_ProfileBadge_Upgraded" />
			<DOTAProfileBadge id="ProfileBadge" />
			<Button id="OkButton" class="PopupButton" onactivate="UIPopupButtonClicked()">
				<Label text="#DOTA_Ok" />
			</Button>
		</Panel>
	</PopupProfileLevelUp>
</root>
