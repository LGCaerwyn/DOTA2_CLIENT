<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/battle_pass/current_battle_pass.css" />
		<include src="file://{resources}/styles/popups/popup_ui_battle_pass_log_entry.css" />
	</styles>

  <DOTABattlePassLogEntry class="BattlePassLogEntry" >
    
    
    <Label class="LevelColumn" text="{d:battle_pass_level}"/>
	<Label class="DateColumn" text="{t:s:timestamp}"/>
	  <Panel class="TypeIcon"/>
    <Label class="DescriptionColumn" text="{s:description}" html="true"/>
	  <Label class="PointsColumn" text="{d:points_granted}"/>
	  <Label class="TotalPointsColumn" text="{d:total_points}"/>
	  
  </DOTABattlePassLogEntry>
</root>

