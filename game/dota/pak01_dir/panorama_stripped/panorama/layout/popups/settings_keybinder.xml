<root>
  <styles>
    <include src="file://{resources}/styles/dotastyles.css" />
    <include src="file://{resources}/styles/popups/settings_keybinder.css" />
  </styles>

  <DOTASettingsKeyBinder class="BindingRow">

    <Label id="title" text="" class="BindingRowLabel" html="true"/>
    <Panel id="LabelFXContainer">
      <Panel id="BindingLabelContainer">
        <Label id="mod" text="" class="BindingRowButton"/>
        <Label id="dash" text="-" class="BindingRowButton"/>
        <Label id="value" text="" class="BindingRowButton"/>
      </Panel>
      <Button class="ClearKeybinding" onactivate="DOTAClearKeybinding()" />
    </Panel>

  </DOTASettingsKeyBinder>

</root>
