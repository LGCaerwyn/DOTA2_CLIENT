<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/popups/popups_shared.css" /> 
		<include src="file://{resources}/styles/battle_pass/international2022/bp_international2022_shared_part2.css" />
		<include src="file://{resources}/styles/popups/splash_ads/popup_international2022_primal_beast_prestige.css" />   
	</styles>

	<Popup class="PopupPanel Hidden" popupbackground="dim" oncancel="UIPopupButtonClicked()">

		<Panel id="Contents">

			<Panel class="ContentsMain">
				<Label class="Title" text="#DOTA_Set_Age_of_Attrition" />
				<Label class="Subtitle" text="#DOTA_BattlePass2022_Schedule_PrimalBeastPrestige_Heading" />
				<Label class="Lore" text="#DOTA_Item_Desc_Age_of_Attrition"/>

				<Panel class="FillHeight" />
				<Panel class="AvailableDetails">
					<Label class="NowAvailable" text="#DOTA_NowAvailable" />
					<Label class="BattlePassLevel" text="#DOTA_PrimalBeastPrestige_Level" />
					<TextButton class="BattlePassBrightButton" text="#DOTA_LearnMore" onactivate="UIPopupButtonClicked( DOTAShowItemDefPage( 23767 ) )" />
				</Panel>
			</Panel>

			<Panel class="CloseButton" onactivate="UIPopupButtonClicked()" />

		</Panel>

		<DOTAUIEconSetPreview class="Model" hittest="false" hittestchildren="false" itemdef="23767" drawbackground="false" displaymode="loadout_small" require-composition-layer="true" pin-fov="vertical" />

	</Popup>
</root>
