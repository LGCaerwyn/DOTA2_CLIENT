<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		                                                                             
		<include src="file://{resources}/styles/battle_pass/international2022/bp_international2022_shared_part2.css" />
		<include src="file://{resources}/styles/dashboard_page_sticker_shared.css" />
		<include src="file://{resources}/styles/popups/splash_ads/popup_international2022_stickers.css" />   
	</styles>

	<Popup class="PopupPanel Hidden" popupbackground="dim" oncancel="UIPopupButtonClicked()">

		<Panel id="Contents">			
			<Label class="SubTitle" text="#DOTA_StickerPopup_NewStickers_Subtitle" />
			<Label class="Title HeaderText" text="#DOTA_StickerPopup_NewStickers_Title" />
			<Label class="Description" html="true" text="#DOTA_StickerPopup_NewStickers_Description" />
			<Button id="PrimaryButton" class="PopupButton BattlePass2022ButtonPrimary" onactivate="UIPopupButtonClicked(); DOTAShowBPInternational2022StickerStorePageWithCategory( Featured ); ">
				<Label text="#DOTA_StickerPopup_NewStickers_ActionLabel" />
			</Button>
			<Label id="SecondaryButton" text="#DOTA_StickerPopup_NewStickers_AboutStickers" onactivate="UIPopupButtonClicked(); DOTAShowBPInternational2022StickerSplashPage();" />
		</Panel>

		<Button class="CloseButton Small" onactivate="UIPopupButtonClicked()" />

	</Popup>
</root>
