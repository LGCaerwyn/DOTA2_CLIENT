<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		                                                                             
		<include src="file://{resources}/styles/battle_pass/international2022/bp_international2022_shared_part2.css" />
		<include src="file://{resources}/styles/dashboard_page_sticker_shared.css" />
		<include src="file://{resources}/styles/popups/splash_ads/popup_international2022_swag_bag.css" />   
	</styles>

	<Popup class="PopupPanel Hidden" popupbackground="dim" oncancel="UIPopupButtonClicked()">

		<Panel id="SwagArt" />

		<Panel id="Contents">
			<Label class="Title HeaderText" text="#DOTA_StickerPopup_SwagBag_Title" />
			<Label class="Description" html="true" text="#DOTA_StickerPopup_SwagBag_Description" />
			<Button id="PrimaryButton" class="PopupButton BattlePass2022ButtonPrimary" onactivate="UIPopupButtonClicked(); DOTAShowBPInternational2022SwagBagPage(); ">
				<Label text="#DOTA_International2022_Swagbag_ViewSwagButtonLabel" />
			</Button>
		</Panel>

		<Button class="CloseButton Small" onactivate="UIPopupButtonClicked()" />

	</Popup>
</root>
