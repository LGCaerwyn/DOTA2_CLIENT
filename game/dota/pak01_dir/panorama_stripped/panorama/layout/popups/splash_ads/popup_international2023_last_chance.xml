<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/popups/popups_shared.css" />
		<include src="file://{resources}/styles/battle_pass/international2023/bp_international2023_shared.css" />
		<include src="file://{resources}/styles/popups/splash_ads/popup_international2023_last_chance.css" />
	</styles>

	<Panel class="PopupPanel LastChancePopup Hidden" popupbackground="dim" oncancel="UIPopupButtonClicked()">

		                                          

		<DOTAParticleScenePanel class="CompediumBackgroundParticles" hittest="false" particleName="particles/ui/compendium/ui_compendium_2023_home_background.vpcf" pin-fov="horizontal" renderdeferred="true" particleonly="false" startActive="true" cameraOrigin="200 0 0" lookAt="0 0 0" fov="60" squarePixels="true" />
		<DOTAParticleScenePanel class="CompediumBackgroundParticles2" hittest="false" particleName="particles/ui/compendium/ui_compendium_2023_home_background.vpcf" pin-fov="horizontal" renderdeferred="true" particleonly="false" startActive="true" cameraOrigin="200 0 0" lookAt="0 0 0" fov="60" squarePixels="true" />

		<Panel class="Contents">

			<Panel class="LogoContainer">
				<DOTAScenePanel class="TopRightGodRays" map="scenes/loadout_preview_rays" camera="camera_1" hittest="false" particleonly="false"/>
				<Image class="Logo" scaling="stretch-to-fit-preserve-aspect" src="file://{images}/logos/ti_logo_2023.png" />
				<Label class="Title" text="#FrontPage_Compendium_Points_Header" />
			</Panel>

			<Label class="EndingDate" text="#DOTA_International2023_LastChancePopup_PopupTitle" />

			<Panel id="StarsContainer">
				<Panel class="CompendiumStar"/>
				<Panel class="CompendiumStar"/>
				<Panel class="CompendiumStar"/>
				<Panel class="CompendiumStar"/>
				<Panel class="CompendiumStar"/>
			</Panel>
			
			<Panel class="Sections">
				<Panel class="CompendiumSection PopupSection">
					<Panel class="TopDash"/>
					<Panel class="SectionImage"/>
					<Panel class="SectionHeader">
						<Label class="SectionTitle" text="#DOTA_International2023_LastChancePopup_RewardsSection_Title" />
						                                                                                  
					</Panel>
					<Panel class="SectionContents">
						<Panel class="SectionFeatures">
							<Label class="LighterGreyText" text="#DOTA_International2023_LastChancePopup_RewardsSection_Description" />
						</Panel>
						<Button class="ButtonPrimary Large TI23Blue" onactivate="DOTAShowBPInternational2023RewardsPage() UIPopupButtonClicked()">
							<Label text="#DOTA_International2023_LastChancePopup_RewardsSection_ButtonLabel"/>
						</Button>
					</Panel>					
					<Panel class="InnerBorder" hittest="false"/>
				</Panel>					
				<Panel class="TalentSection PopupSection">
					<Panel class="TopDash"/>
					<Panel class="SectionImage"/>
					<Panel class="SectionHeader">
						<Label class="SectionTitle" text="#DOTA_International2023_LastChancePopup_TalentSection_Title" />
						                                                                              
					</Panel>
					<Panel class="SectionContents">
						<Panel class="SectionFeatures">
							<Label class="LighterGreyText" text="#DOTA_International2023_LastChancePopup_TalentSection_Description" />
						</Panel>
						<Button class="ButtonPrimary Large TI23Blue" onactivate="DOTAShowBPInternational2023TalentPage() UIPopupButtonClicked()">
							<Label text="#DOTA_International2023_LastChancePopup_TalentSection_ButtonLabel"/>
						</Button>
					</Panel>
					<Panel class="InnerBorder" hittest="false"/>
				</Panel>
				<Panel class="SupportersClubSection PopupSection">
					<Panel class="TopDash"/>
					<Panel class="SectionImage"/>
					<Panel class="SectionHeader">
						<Label class="SectionTitle" text="#DOTA_International2023_LastChancePopup_TeamsSection_Title" />
						                                                                             
					</Panel>
					<Panel class="SectionContents">
						<Panel class="SectionFeatures">
							<Label class="LighterGreyText" text="#DOTA_International2023_LastChancePopup_TeamsSection_Description" />
						</Panel>
						<Button class="ButtonPrimary Large TI23Blue" onactivate="DOTAShowBPInternational2023SupportersClub() UIPopupButtonClicked()">
							<Label text="#DOTA_International2023_LastChancePopup_TeamsSection_ButtonLabel"/>
						</Button>
					</Panel>
					<Panel class="InnerBorder" hittest="false"/>
				</Panel>
			</Panel>
		</Panel>

		<Button class="CloseButton" onactivate="UIPopupButtonClicked()" />

	</Panel>
</root>
