<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/purchase_button_multi_option.css" />
	</styles>

	<DOTAPurchaseButtonMultiOption>
		<Panel class="PurchaseButtonMultiOptionContainer">
			<Panel class="OptionContainer">
				<DOTAPurchaseButton id="PrimaryPurchaseButton" />
				<DropDown id="PrimaryQuantityDropDown" class="QuantityDropDown">
					                              
				</DropDown>
			</Panel>
			<DOTAPurchaseButton id="SecondaryPurchaseButton" onmouseover="DOTAPurchaseButtonMultiOptionExpandSecondary()" onmouseout="DOTAPurchaseButtonMultiOptionCollapseSecondary()"/>
		</Panel>
	</DOTAPurchaseButtonMultiOption>
</root>
