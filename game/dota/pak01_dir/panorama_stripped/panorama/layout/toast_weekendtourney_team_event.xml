<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/toast_weekendtourney_team_event.css" />
	</styles>

	<DOTAWeekendTourneyTeamEventToast>
		<Panel id="TeamContainer" >
			<DOTAUGCImage id="TeamLogo" />
			<Panel id="TeamIdentity">
				<Label id="TeamName" text="{s:team_name}"/>
				<Panel id="AvatarRow">
					                              
				</Panel>
			</Panel>
		</Panel>
		
		<Label id="EventDescription" text="{s:event_description}" />
		
		<Panel id="BracketButtonContainer">
			<Button id="ShowTourneyButton" class="BlueButton">
				<Label text="#WeekendTourneyToast_ShowTourney"/>
			</Button>
		</Panel>
	</DOTAWeekendTourneyTeamEventToast>
</root>
