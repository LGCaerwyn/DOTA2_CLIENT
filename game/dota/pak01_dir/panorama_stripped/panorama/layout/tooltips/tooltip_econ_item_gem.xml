<root>
  <styles>
    <include src="file://{resources}/styles/dotastyles.css" />
    <include src="file://{resources}/styles/tooltips/tooltip_econ_item.css" />
  </styles>
  <Panel class="EcontooltipGem LeftRightFlow">		
    <Panel class="GemIconContainer">
	  <Panel class="GemIcon">
      <EconItemImage id="GemIconImage" src="file://{images}/econ/sockets/gem_stat.png" scaling="stretch-to-cover-preserve-aspect"/>
      <Image class="EmptyGemIconImage" src="file://{images}/econ/sockets/gem_stat_empty.png" scaling="stretch-to-cover-preserve-aspect"/>
      <Image id="GemIconOverlay" src="file://{images}/textures/gem_overlay.psd" scaling="stretch-to-cover-preserve-aspect"/>
	  </Panel>
	</Panel>
	<Panel class="GemDetails TopBottomFlow GemInscribed">
	  <Panel class="GemName LeftRightFlow">
	    <Label id="GemCount" text="#DOTA_tooltip_econ_item_gem_strange_count"/>
		<Label text="#DOTA_tooltip_econ_item_gem_description"/>
	  </Panel>
	  <Panel class="GemType">
	    <Label text="#DOTA_tooltip_econ_item_gem_type"/>
	  </Panel>
	</Panel>
  </Panel>
</root>