<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/tooltips/tooltip_graph_killinfo.css" />
		<include src="file://{resources}/styles/tooltips/tooltip_ability.css" />
	</styles>

	<DOTATooltipGraphKillInfo hittest="false" >
		<Panel class="Content" >
			<Panel id="KillDetails">
				<DOTAHeroImage id="KillerImage" heroimagestyle="icon" />
				<DOTAUserName id="KillerPlayerName" class="KillerText" />
				<Label id="KillerTeamName" class="KillerText" />
				<Panel id="SwordIcon" />
				<DOTAHeroImage id="VictimImage" heroimagestyle="icon" />
				<DOTAUserName id="VictimPlayerName" class="VictimText" />
				<Panel id="VictimStructureIcon" />
				<Label id="Bounty" text="{d:bounty}" />
				<Panel id="GoldIcon" />
			</Panel>
			<Panel class="HorizontalCenter JumpRow">
				<Panel class="LeftRightFlow HorizontalCenter">
					<Label id="JumpLabel" text="#dota_page_match_details_kill_info_jump_to_event" />
					<Image class="ArrowIcon" src="file://{images}/control_icons/24px/arrow_right.svg" scaling="stretch-to-fit-preserve-aspect" />
				</Panel>
			</Panel>
		</Panel>
	</DOTATooltipGraphKillInfo>
</root>
