<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/tooltips/tooltip_hud_player_mute.css" />
	</styles>
	
	<Panel class="PlayerMuteTooltipContents">
		<Panel class="MuteAndReportContents">
			<Label class="Title" text="#DOTA_Scoreboard_Mute_Title" />
			<Label class="ActionText" text="#DOTA_Scoreboard_Mute_Desc" />
			<Label class="ClickHint" text="#DOTA_Scoreboard_Mute_RightClickHint" />
		</Panel>

		<Panel class="MuteStatusContents">
			<Label class="Title" text="#DOTA_Scoreboard_PartiallyMuted_Title" />

			<Panel class="MuteStatusTypes">
				<Panel id="TextMuteStatus" class="MuteStatus">
					<Panel class="MuteIcon" />
					<Label text="#DOTA_Scoreboard_PartiallyMuted_Text" />
				</Panel>
				<Panel id="VoiceMuteStatus" class="MuteStatus">
					<Panel class="MuteIcon" />
					<Label text="#DOTA_Scoreboard_PartiallyMuted_Voice" />
				</Panel>
			</Panel>
			<Label class="ClickHint" text="#DOTA_Scoreboard_PartiallyMuted_ClickHint" />
		</Panel>
	</Panel>
</root>
