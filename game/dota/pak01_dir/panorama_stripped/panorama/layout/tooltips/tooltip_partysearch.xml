<root>
	<styles>
		<include src="file://{resources}/styles/gamestyles.css" />
		<include src="file://{resources}/styles/tooltips/tooltip_partysearch.css" />
	</styles>

	<TooltipCustomLayout class="TTPartySearch" onload="DOTASetPartyFinderDialogVars()">
		<Panel class="TTPartySearch_Layout">
			<Panel class="TTPartySearch_Title">
				<Panel class="TTPartySearch_Icon" />
				<Label class="TTPartySearch_Title_Label" text="#PartySearch_Tooltip_Title" />
			</Panel>
			<Panel class="TTPartySearch_Entitlement">
				<Label class="TTPartySearch_Entitlement_Label" text="#PartySearch_Tooltip_Entitlement" />
				<Panel class="TTPartySearch_Entitlement_Icon" />
			</Panel>
			<Label class="TTPartySearch_Desc" text="#PartySearch_Tooltip_Desc" />
			<Label class="TTPartySearch_Avail" text="#PartySearch_Tooltip_Avail" />
		</Panel>
	</TooltipCustomLayout>
</root>
