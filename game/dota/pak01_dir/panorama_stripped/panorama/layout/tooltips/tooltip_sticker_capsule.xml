<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/tooltips/tooltip_sticker_capsule.css" />
	</styles>

	<DOTATooltipStickerCapsule>
		<Panel class="StickerCapsuleContents">

			                                
				                                                           
			        

			                            
				                                                               
			           

			<Label class="OddsLabel" text="#DOTA_tooltip_sticker_capsule_generic"/>

			<Panel class="OddsList">
				<Panel class="OddItem">
					<Label class="OddsName" text="#unique"/>
					<Label class="OddsValue" text="{s:sticker_odds_unique}%"/>
				</Panel>
				<Panel class="OddItem">
					<Label class="OddsName" text="#glitter"/>
					<Label class="OddsValue" text="{s:sticker_odds_glitter}%"/>
				</Panel>
				<Panel class="OddItem">
					<Label class="OddsName" text="#holo"/>
					<Label class="OddsValue" text="{s:sticker_odds_holo}%"/>
				</Panel>
				<Panel class="OddItem">
					<Label class="OddsName" text="#gold"/>
					<Label class="OddsValue" text="{s:sticker_odds_gold}%"/>
				</Panel>
			</Panel>
		</Panel>
	</DOTATooltipStickerCapsule>
</root>
