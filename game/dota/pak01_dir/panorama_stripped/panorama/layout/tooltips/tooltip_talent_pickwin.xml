<root>
	<styles>
	<include src="file://{resources}/styles/dotastyles.css" />
	<include src="file://{resources}/styles/tooltips/tooltip_talent_pickwin.css" />
	</styles>

	<DOTATooltipTalentPickWin class="PickWinContents">
		<Panel class="TopBottomFlow">
			<Label id="Header" text="{s:talent_name}"/>
			<Panel class="PickWinRow">
				<Label class="PickWinLabel" text="#DOTA_HeroStatTooltip_WinPercent"/>
				<Label class="PercentageLabel" text="{s:win_percent}%"/>
			</Panel>
			<Panel class="PickWinRow">
				<Label class="PickWinLabel" text="#DOTA_HeroStatTooltip_PickPercent"/>
				<Label class="PercentageLabel" text="{s:pick_percent}%"/>
			</Panel>
		</Panel>
	</DOTATooltipTalentPickWin>
</root>

