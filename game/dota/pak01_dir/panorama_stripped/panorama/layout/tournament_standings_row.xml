<root>
	<styles>
    <include src="file://{resources}/styles/dotastyles.css" />
    <include src="file://{resources}/styles/dashboard.css" />
    <include src="file://{resources}/styles/tournament_group.css" />
	</styles>

  <TournamentStandingsRow class="GroupRow">
    <Panel id="GroupCellRank">
      <Label class="GroupCellRankLabel" text="#DOTA_TournamentBracket_RowRank"/>
    </Panel>
    <Panel id="GroupCellTeam">
      <DOTAUGCImage id="GroupCellTeamLogo" class="GroupCellTeamLogo"/>
      <Label class="GroupCellTeamName" text="#DOTA_TournamentBracket_RowTeamName"/>
    </Panel>
    <Panel id="GroupCellGamesPlayed">
      <Label class="GroupCellGamesPlayedLabel" text="#DOTA_TournamentBracket_RowGamesPlayed"/>
    </Panel>
    <Panel id="GroupCellRecord">
      <Label class="GroupCellGamesPlayedLabel" text="#DOTA_TournamentBracket_RowPoints"/>
    </Panel>
 
    <Panel id="GroupCellScore">
      <Label class="GroupCellScoreLabel" text="#DOTA_TournamentBracket_Score"/>
    </Panel>
  </TournamentStandingsRow>
</root>

