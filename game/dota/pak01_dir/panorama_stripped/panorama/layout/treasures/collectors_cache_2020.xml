<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/battle_pass/international2020/bp_international2020_shared.css" />
		<include src="file://{resources}/styles/treasures/collectors_cache_2020.css" />
	</styles>

	<Panel class="RootPanel" hittest="false">
		<Panel class="BonusLevels">
			<Panel id="PanelBg">

				<Panel id="PanelHeader">
					<Label class="header-text" text="#DOTA_TI10_CollectorsCache_HeaderText" />
				</Panel>

				<Panel id="AchievementProgress">
					<Label class="sub-text" html="true" text="#DOTA_TI10_CollectorsCache_SubText" />
					<DOTAEventActionCount eventid="29" actionid="55" layoutfile="file://{resources}/layout/treasures/collectors_cache_2020_counter.xml" setclassidprefix="ProgressTick_" setclass="Ticked"/>
				</Panel>

			</Panel>
			<Label class="FooterText" text="#DOTA_EconItemSet_Tooltip_Exclusive_TI10_Collectors_Cache" />
		</Panel>
		<DOTACountdown id="Countdown" end-time="1596481200" server-time="true" hittest="false" hittestchildren="false">
			<Label class="time-left" text="{t:d:t:T:countdown_time}" />
			<Label class="remaining" text="#DOTA_TI10_Collectors_Cache_Remaining" />
			<Label class="finished" text="#DOTA_TI10_Collectors_Cache_Finished" />
		</DOTACountdown>
	</Panel>
</root>
