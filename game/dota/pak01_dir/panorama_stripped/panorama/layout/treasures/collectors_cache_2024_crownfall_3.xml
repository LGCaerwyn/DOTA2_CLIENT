<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/treasures/collectors_cache_2024_crownfall.css" />
	</styles>

	<Panel class="RootPanel">
		<Panel class="LogoContainer">
			<DOTAParticleScenePanel class="ParticleWindow" particleName="particles\econ\events\frostivus_2023\dashboard_treasure_ambient_dryice.vpcf" renderdeferred="true" particleonly="false" startActive="true" cameraOrigin="0 700 0" lookAt="0 60 -26" fov="10" squarePixels="true"/>
			<Image class="TitleLogoBackgroundImage" src="file://materials/models/ui/loadout/collectorscache_2024_image_standlone.psd" scaling="stretch-to-fit-preserve-aspect" />
			<Panel class="LogoShadow"/>
			<Panel class="TitleTextContainer">
				<Image class="CrownfallLogo" src="file://{images}/events/crownfall/logos/crownfall_logo_en.psd" scaling="stretch-to-fit-preserve-aspect" />
				<Image class="CrownfallLogoCN" src="file://{images}/events/crownfall/logos/crownfall_logo_cn.psd" scaling="stretch-to-fit-preserve-aspect" />
				<Label class="ContestTitle TitleExtraLarge" html="true" text="#DOTA_Winter2024_HeroesHoard_Title" />
				<Label class="ContestDescription" text="#DOTA_Winter2024_HeroesHoard_Cell_Description" />
			</Panel>
		</Panel>
		<DOTAParticleScenePanel class="SmokeFX Bottom" hittest="false" particleName="particles/ui/battle_pass/ui_bp_2022_diretide_home_background.vpcf" require-composition-layer="true" renderdeferred="true" particleonly="false" startActive="true" cameraOrigin="200 0 0" lookAt="0 0 0" fov="60" squarePixels="true" />
	</Panel>
</root>
