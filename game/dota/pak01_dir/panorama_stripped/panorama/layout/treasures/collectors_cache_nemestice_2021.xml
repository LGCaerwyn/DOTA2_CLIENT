<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/battle_pass/spring2021/bp_spring2021_shared.css" />
		<include src="file://{resources}/styles/treasures/collectors_cache_nemestice_2021.css" />
	</styles>

	<Panel class="RootPanel" hittest="false">
		<Panel class="BonusLevels">
			<Panel id="PanelBg">

				<Panel id="PanelHeader">
					<Label class="header-text" text="#DOTA_Nemestice2021_CollectorsCache_HeaderText" />
				</Panel>

				<Panel id="AchievementProgress">
					<Label class="sub-text" html="true" text="#DOTA_Nemestice2021_CollectorsCache_SubText" />
					<DOTAEventActionCount eventid="32" actionid="55" layoutfile="file://{resources}/layout/treasures/collectors_cache_nemestice_2021_counter.xml" setclassidprefix="ProgressTick_" setclass="Ticked"/>
				</Panel>

			</Panel>
		</Panel>
		<DOTACountdown id="Countdown" end-time="1629226800" server-time="true" hittest="false" hittestchildren="false">
			<Label class="time-left" text="{t:d:t:T:countdown_time}" />
			<Label class="remaining" text="#DOTA_TI9_Collectors_Cache_Remaining" />
			<Label class="finished" text="#DOTA_TI9_Collectors_Cache_Finished" />
		</DOTACountdown>
	</Panel>
</root>
