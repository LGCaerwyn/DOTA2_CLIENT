<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/battle_pass/current_battle_pass.css" />
		<include src="file://{resources}/styles/ui_change_log_entry.css" />
	</styles>

	<Panel class="ChangeLogEntry" >
		<DOTAUGCImage id="GuildLogo" class="TypeIcon"/>
		<Panel class="LogContainer">
			<Label id="Description" text="{s:description}" html="true"/>
			<Panel class="BottomLine" >
				<Panel class="TimeContainer LeftRightFlow">
					<Panel class="TimeIcon"/>
					<Label class="Date" text="{t:s:timestamp}"/>
				</Panel>
				<Panel class="ViewLog" />
			</Panel>
		</Panel>
	</Panel>
</root>

