<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/dashboard_page_shared.css" />
		<include src="file://{resources}/styles/ui_sticker_collection_page.css" />
		<include src="file://{resources}/styles/quick_search.css" />
		<include src="file://{resources}/styles/ui_econ_item.css" />
	</styles>

	<DOTAStickerCollectionPage>
		<DOTATeamImage id="TeamImage" class="TeamImage" teamimagestyle="squarelarge" teamtooltipstyle="none"/>
		<Panel class="TeamIdentification">
			<Label class="SidebarName TeamOnly" text="{g:dota_team_name:team_id}"/>
			<Label class="SidebarName TalentOnly" text="#DOTA_StickerCollections_TalentPageName"/>
			<Button id="FavoriteTeam" class="FavoriteTeam" onmouseover="UIShowTextTooltip( #DOTA_StickerCollections_Favorite_Tooltip )" onmouseout="UIHideTextTooltip()" />
		</Panel>
		<Button id="DeleteButton" class="DeleteRow"/>
		<Button id="UpButton" class="DeleteRow"/>
		<Button id="DownButton" class="DeleteRow"/>
		<Panel class="BottomRow">
			<Panel id="TeamSticker" class="StickerSlot" hittest="true">
				<Panel class="EmptyPlayerSticker TalentOnly" hittest="false"/>
				<Panel class="EmptyTeamSticker TeamOnly" hittest="false"/>
				<Image id="StickerItemShadow" class="Sticker PlayerSticker HideStickerbacks" hittest="false" />
				<Button class="DetailsButton InspectIcon" id="StickerDetailsButton" hittest="true"/>
				<DOTAUIEconSetPreview id="StickerItem" class="Sticker PlayerSticker HideStickerbacks" onmouseout="DOTAHideEconItemTooltip()"  hittest="false" rotateonmousemove="true" rotationlimits="1.5 -1.5 1.5 -1.5" stickeranim="stock_idle" yawaxis="pitch" pitchaxis="roll" stickermap="scenes/stickers/sticker_basic_locked" hittestchildren="false"/>
			</Panel>
			<Panel id="PlayerSticker0" class="StickerSlot" hittest="true">
				<Panel class="EmptyPlayerSticker" hittest="false"/>
				<Image id="StickerItemShadow" class="Sticker PlayerSticker " hittest="false" />
				<Button class="DetailsButton InspectIcon" id="StickerDetailsButton" hittest="true"/>
				<DOTAUIEconSetPreview id="StickerItem" class="Sticker PlayerSticker HideStickerbacks" onmouseout="DOTAHideEconItemTooltip()"  hittest="false" rotateonmousemove="true" rotationlimits="1.5 -1.5 1.5 -1.5" stickeranim="stock_idle" yawaxis="pitch" pitchaxis="roll" stickermap="scenes/stickers/sticker_basic_locked" hittestchildren="false"/>
			</Panel>
			<Panel id="PlayerSticker1" class="StickerSlot" hittest="true">
				<Panel class="EmptyPlayerSticker" hittest="false"/>
				<Image id="StickerItemShadow" class="Sticker PlayerSticker " hittest="false" />
				<Button class="DetailsButton InspectIcon" id="StickerDetailsButton" hittest="true"/>
				<DOTAUIEconSetPreview id="StickerItem" class="Sticker PlayerSticker HideStickerbacks" onmouseout="DOTAHideEconItemTooltip()"  hittest="false" rotateonmousemove="true" rotationlimits="1.5 -1.5 1.5 -1.5" stickeranim="stock_idle" yawaxis="pitch" pitchaxis="roll" stickermap="scenes/stickers/sticker_basic_locked" hittestchildren="false"/>
			</Panel>
			<Panel id="PlayerSticker2" class="StickerSlot" hittest="true">
				<Panel class="EmptyPlayerSticker" hittest="false"/>
				<Image id="StickerItemShadow" class="Sticker PlayerSticker " hittest="false" />
				<Button class="DetailsButton InspectIcon" id="StickerDetailsButton" hittest="true"/>
				<DOTAUIEconSetPreview id="StickerItem" class="Sticker PlayerSticker HideStickerbacks" onmouseout="DOTAHideEconItemTooltip()"  hittest="false" rotateonmousemove="true" rotationlimits="1.5 -1.5 1.5 -1.5" stickeranim="stock_idle" yawaxis="pitch" pitchaxis="roll" stickermap="scenes/stickers/sticker_basic_locked" hittestchildren="false"/>
			</Panel>
			<Panel id="PlayerSticker3" class="StickerSlot" hittest="true">
				<Panel class="EmptyPlayerSticker" hittest="false"/>
				<Image id="StickerItemShadow" class="Sticker PlayerSticker " hittest="false" />
				<Button class="DetailsButton InspectIcon" id="StickerDetailsButton" hittest="true"/>
				<DOTAUIEconSetPreview id="StickerItem" class="Sticker PlayerSticker HideStickerbacks" onmouseout="DOTAHideEconItemTooltip()"  hittest="false" rotateonmousemove="true" rotationlimits="1.5 -1.5 1.5 -1.5" stickeranim="stock_idle" yawaxis="pitch" pitchaxis="roll" stickermap="scenes/stickers/sticker_basic_locked" hittestchildren="false"/>
			</Panel>
			<Panel id="PlayerSticker4" class="StickerSlot" hittest="true">
				<Panel class="EmptyPlayerSticker" hittest="false"/>
				<Image id="StickerItemShadow" class="Sticker PlayerSticker " hittest="false" />
				<Button class="DetailsButton InspectIcon" id="StickerDetailsButton" hittest="true"/>
				<DOTAUIEconSetPreview id="StickerItem" class="Sticker PlayerSticker HideStickerbacks" onmouseout="DOTAHideEconItemTooltip()"  hittest="false" rotateonmousemove="true" rotationlimits="1.5 -1.5 1.5 -1.5" stickeranim="stock_idle" yawaxis="pitch" pitchaxis="roll" stickermap="scenes/stickers/sticker_basic_locked" hittestchildren="false"/>
			</Panel>
		</Panel>
	</DOTAStickerCollectionPage>


</root>
